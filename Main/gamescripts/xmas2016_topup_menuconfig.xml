<Attributes>
	<MenuConfig>
		<Attribute name="Objects">
			<AttributeSet name="darken">
				<Attribute name="active" formula="0" />
			</AttributeSet>
			<AttributeSet name="TwoButtons">
				<Attribute name="active" value="true" />
			</AttributeSet>
			<AttributeSet name="ConfirmButtonIcon">
				<Attribute name="active" value="true" />
			</AttributeSet>
			<AttributeSet name="OneButtonAndBack">
				<Attribute name="active" value="false" />
			</AttributeSet>
			<AttributeSet name="TitleText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_Upgrade_TopUp_title" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="ResourceMessage_HeadingText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_Upgrade_TopUp_header" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="textReplacements">
					<AttributeSet>
						<Attribute name="index" value="0" />
						<Attribute name="text" formula="GnomeLevel + 1" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="ResourceMessage_AmountText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_Upgrade_TopUp_required" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="color" value="0xFFD51AFF" />
				<Attribute name="textReplacements">
					<AttributeSet>
						<Attribute name="index" value="1" />
						<Attribute name="text" formula="Selector::getValue('XMASSixteenGnomeUpgradeCost')" />
					</AttributeSet>
				</Attribute>
				<Attribute name="iconReplacements">
					<AttributeSet>
						<Attribute name="index" value="0" />
						<Attribute name="icon" value="ico_cur_xmas2016_guiltdust_sm.rgb" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="CurrencyAmount_Text">
				<Attribute name="text" formula="System::evaluate('Spendable::donuts')" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="CurrencyAmount_Icon">
				<Attribute name="image" value="ico_genr_donut_xs:*" />
			</AttributeSet>
			<AttributeSet name="PurchaseMessage_Text">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_Upgrade_TopUp_text" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="textReplacements">
					<AttributeSet>
						<Attribute name="index" value="0" />
						<Attribute name="text" formula="Selector::getValue('XMASSixteenGnomeUpgradeCost') - System::evaluate('Spendable::GuiltDust')" />
					</AttributeSet>
				</Attribute>
				<Attribute name="iconReplacements">
					<AttributeSet>
						<Attribute name="index" value="1" />
						<Attribute name="icon" value="ico_cur_xmas2016_guiltdust_sm.rgb" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="ConfirmArrowT">
				<Attribute name="color" value="D03484FF" />
			</AttributeSet>
			<AttributeSet name="ConfirmArrowBRect">
				<Attribute name="color" value="D03484FF" />
			</AttributeSet>
			<AttributeSet name="ConfirmText">
				<Attribute name="text" formula="ceil((Selector::getValue('XMASSixteenGnomeUpgradeCost') - System::evaluate('Spendable::GuiltDust'))*toDouble(System::getConfigValue('__XMAS2016_Spreadsheet_Config:Resources:GuiltDust_DonutCost__')))" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="ConfirmTexthighlight">
				<Attribute name="text" formula="ceil((Selector::getValue('XMASSixteenGnomeUpgradeCost') - System::evaluate('Spendable::GuiltDust'))*toDouble(System::getConfigValue('__XMAS2016_Spreadsheet_Config:Resources:GuiltDust_DonutCost__')))" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="ConfirmButton_Icon">
				<Attribute name="image" value="ico_genr_donut_xs:*" />
			</AttributeSet>
			<AttributeSet name="GoToButtonTexthighlight">
				<Attribute name="text" value="UI_Cancel" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="GoToButtonText">
				<Attribute name="text" value="UI_Cancel" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
		</Attribute>
	</MenuConfig>
	<ButtonsConfig>
		<Attribute name="Buttons">
			<AttributeSet name="ConfirmButton">
				<Attribute name="script" value="XMAS2016_GnomeScripts:DoDonutUpgradeGnome" />
			</AttributeSet>
			<AttributeSet name="GoToButton">
				<Attribute name="script" value="XMAS2016_GnomeScripts:HideTopUp" />
			</AttributeSet>
		</Attribute>
	</ButtonsConfig>
</Attributes>