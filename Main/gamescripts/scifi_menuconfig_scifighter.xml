								<Requirement type="reqList" location="SciFi_GameConfig:Requirement_Act2" />
							</Attribute>
							<Attribute name="icon" value="ico_cur_scifi2016_matter_sm.rgb">
								<Requirement type="reqList" location="SciFi_GameConfig:Requirement_Act3" />
							</Attribute>
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="2" />
							<Attribute name="icon" value="ico_cur_scifi2016_robotparts_sm.rgb" />
						</AttributeSet>
					</Attribute>
					<Attribute name="iconReplacements">
						<Requirement type="variable" variable="SF_FBAvailable" />
						<AttributeSet>
							<Attribute name="index" value="0" />
							<Attribute name="icon" value="ico_cur_scifi2016_darkmatter_sm.rgb" />
							<Attribute name="icon" value="ico_cur_scifi2016_time_sm.rgb">
								<Requirement type="reqList" location="SciFi_GameConfig:Requirement_Act2" />
							</Attribute>
							<Attribute name="icon" value="ico_cur_scifi2016_matter_sm.rgb">
								<Requirement type="reqList" location="SciFi_GameConfig:Requirement_Act3" />
							</Attribute>
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="2" />
							<Attribute name="icon" value="ico_cur_scifi2016_robotparts_sm.rgb" />
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="4" />
							<Attribute name="icon" value="ico_cur_scifi2016_futurebucks_sm.rgb" />
						</AttributeSet>
					</Attribute>
					<Attribute name="textReplacements">
						<AttributeSet>
							<Attribute name="index" value="1" />
							<Attribute name="text" formula="(SF_BaseReward + SF_CompletionReward) + ((SF_PerSecondReward*(SF_GameLength-SF_FRAMECNT)) + ((SF_RobotRewards*SF_RobotEvent)+ (SF_AdvRobotsRewards*SF_AdvRobotEvent)))" />
						</AttributeSet>
					</Attribute>
					<Attribute name="textReplacements">
						<Requirement type="variable" variable="SF_RPAvailable" />
						<AttributeSet>
							<Attribute name="index" value="1" />
							<Attribute name="text" formula="(SF_BaseReward + SF_CompletionReward) + ((SF_PerSecondReward*(SF_GameLength-SF_FRAMECNT)) + ((SF_RobotRewards*SF_RobotEvent)+ (SF_AdvRobotsRewards*SF_AdvRobotEvent)))" />
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="3" />
							<Attribute name="text" formula="(SF_BaseReward_RP + SF_CompletionReward_RP) + ((SF_PerSecondReward_RP*(SF_GameLength-SF_FRAMECNT)) + (ceil((SF_RobotRewards*SF_RobotCraft1)*SF_RobotCraft1Chance)+((SF_AdvRobotsRewards*SF_AdvRobotCraft1)*SF_AdvRobotCraft1Chance)))" />
						</AttributeSet>
					</Attribute>
					<Attribute name="textReplacements">
						<Requirement type="variable" variable="SF_FBAvailable" />
						<AttributeSet>
							<Attribute name="index" value="1" />
							<Attribute name="text" formula="SF_BaseReward + ((SF_PerSecondReward*(SF_GameLength-SF_FRAMECNT)) + ((SF_RobotRewards*SF_RobotEvent)+ (SF_AdvRobotsRewards*SF_AdvRobotEvent)))" />
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="3" />
							<Attribute name="text" formula="(SF_BaseReward_RP + SF_CompletionReward_RP) + ((SF_PerSecondReward_RP*(SF_GameLength-SF_FRAMECNT)) + (ceil((SF_RobotRewards*SF_RobotCraft1)*SF_RobotCraft1Chance)+((SF_AdvRobotsRewards*SF_AdvRobotCraft1)*SF_AdvRobotCraft1Chance)))" />
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="5" />
							<Attribute name="text" formula="(SF_BaseReward_FB + SF_CompletionReward_FB) + ((SF_PerSecondReward_FB*(SF_GameLength-SF_FRAMECNT)) + (ceil((SF_RobotRewards*SF_RobotCraft2)*SF_RobotCraft2Chance)+((SF_AdvRobotsRewards*SF_AdvRobotCraft2)*SF_AdvRobotCraft2Chance)))" />
						</AttributeSet>
					</Attribute>
				</Attribute>
			</AttributeSet>
		</Attribute>
	</SciFighterRewardNotificationConfig_Win>
