	<DinosaurTapped_Single parallel="true" cache="true">
		<Action type="if">
			<If expression="bitwiseAnd([Object::ObjectFlags], 1) == 0" asObjectExpression="true">
				<Then>
					<Action type="updateVariable" variable="DinosaursTapped" value="DinosaursTapped + 1" />
					<Action type="setObjectFlag" flags="delete" />
					<Action type="sound" sound="TTT_PterodactylNest_Tap" />
					<Action type="if">
						<If expression="[Object::SkinID]==0" asObjectExpression="true">
							<Then>
								<Action type="anim-play" animation="Squish" />
							</Then>
						</If>
						<ElseIf expression="[Object::SkinID]==249004" asObjectExpression="true">
							<Then>
								<Action type="anim-play" animation="Raptor_Alt_Squish" />
							</Then>
						</ElseIf>
					</Action>
					<Action type="anim-waitforend" />
					<Action type="fade" fade="outmax" />
					<Action type="deletecharacter" character="DinosaurTappable" />
					<Action type="if">
						<If expression="#program;                             if(User::getVariable('BankedDinosaurTappables') &gt; 3)                             {                                 return false;                             }                             else                             {                                 var character = Character::find('DinosaurTappable');                                 var characterInstanceList = Land::getAllInstancesOfObject(character);                                 foreach(characterInstanceList as instance)                                 {                                     if(!Object::getDelete(instance))                                     {                                         return false;                                     }                                 }                             }                             return true;">
							<Then>
								<Action type="createobject" name="DinosaurTappable" count="3" save="true" panCamera="false" />
							</Then>
						</If>
					</Action>
				</Then>
			</If>
		</Action>
	</DinosaurTapped_Single>
	<ClearTappable>
		<Action type="if">
