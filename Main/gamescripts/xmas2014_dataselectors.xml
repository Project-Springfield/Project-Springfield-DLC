<DynamicDataSelectors>
	<Selector name="XMASBerriesPerFriendAction" type="table" key="FriendActionsCount">
		0,5,
		30,3,
		60,2,
		90,1,
		120,0,
	</Selector>
	<Selector name="XMASPresentsPerFriendAction" type="table" key="FriendActionsCount">
		0,5,
		30,3,
		60,2,
		90,1,
		120,0,
	</Selector>
	<Selector name="XMASBerriesPerFriendActionElfCannonTouched" type="formula" formula="2*System::evaluate('Selector::XMASBerriesPerFriendAction')" />
	<Selector name="XMASBerriesPerCleanElfCannon" type="fixed" value="5" />
	<Selector name="CannonCostFormula" type="formula" formula="min(FullBottles,System::evaluate('Selector::CanonFireAmount'))" />
	<Selector name="CannonRewardFormula" type="formula" formula="System::evaluate('Selector::CannonCostFormula')*System::evaluate('Selector::CanonBerriesPerLevel')" />
	<Selector name="JobTimeBonus" type="table" key="Object::WorkshopLevel" usingBuilding="ToyWorkshop">
		0,60,
		1,120,
		2,240,
		3,300,
		4,360,
		5,420,
		6,480,
		7,540,
		8,600,
	</Selector>
	<Selector name="ToyWorkshopPresentsPerLevel" type="table" key="Object::WorkshopLevel" usingBuilding="ToyWorkshop">
		0,60,
		1,120,
		2,240,
		3,300,
		4,360,
		5,420,
		6,480,
		7,540,
		8,600,
	</Selector>
</DynamicDataSelectors>