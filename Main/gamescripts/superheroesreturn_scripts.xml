<ScriptPackage>
	<SuperheroesReturnStoreRecommendation>
		<Action type="recommendation" recType="Event" eventName="UI_SuperheroesReturn" name="EnterSuperheroesReturnCategory" />
	</SuperheroesReturnStoreRecommendation>
	<SuperheroesReturnClearStoreRecommendation>
		<Action type="clearRecommendations" category="Event" name="EnterSuperheroesReturnCategory" />
	</SuperheroesReturnClearStoreRecommendation>
	<EnterSuperheroesReturnCategory>
		<Action type="focusOnMenuItem" menuCategory="SuperheroesReturn" />
	</EnterSuperheroesReturnCategory>
	<DoorbellFiestaVan_Tapped parallel="true">
		<Action type="anim-play" animation="ActiveNoLoop" />
	</DoorbellFiestaVan_Tapped>
	<PiesAndExplosivesPile_Tapped parallel="true">
		<Action type="anim-play" animation="ActiveNoLoop" />
	</PiesAndExplosivesPile_Tapped>
	<AutoconsumeCyborgSnakePurchase_End parallel="true">
		<Action type="consumable" consumable="CyborgSnake" fromInventory="true" />
	</AutoconsumeCyborgSnakePurchase_End>
	<AutoconsumeFruitBatMan_End parallel="true">
		<Action type="consumable" consumable="FruitBatMan" fromInventory="true" />
	</AutoconsumeFruitBatMan_End>
	<PromoteSuperheroesReturnMysteryBox_Start parallel="true">
		<Action type="recommendation" recType="Promoted" name="SuperheroesReturn_MysteryBox" />
	</PromoteSuperheroesReturnMysteryBox_Start>
	<PieManSeasonCheck>
		<Action type="checkRequirements">
			<Requirement type="characterunlocked" name="Homer" unlockedskin="PieMan" />
			<True>
				<Action type="setVariable" boolVariable="PieManSeason1Owned" boolValue="true" />
			</True>
		</Action>
	</PieManSeasonCheck>
	<RadioactiveManBundle_Purchased>
		<Action type="gotoState" state="GameState_InGame" waitForState="true" />
		<Action type="notification" icon="constructionsite_menu" title="BLD_ConstructionSite_name" text="UI_AtW_ItemPurchased" button="UI_OK" />
		<Action type="focusOnMenuItem" itemName="ConstructionSite" inventory="true" />
	</RadioactiveManBundle_Purchased>
	<CyborgSnakeBundle_Purchased>
		<Action type="notification" icon="prison_menu" title="BLD_SpringfieldPenitentiary_name" text="UI_AtW_ItemPurchased" button="UI_OK" />
		<Action type="focusOnMenuItem" itemName="SpringfieldPenitentiary" inventory="true" />
	</CyborgSnakeBundle_Purchased>
	<DrColossusBundle_Purchased>
		<Action type="notification" icon="deathmountain_menu" title="BLD_DeathMountain_name" text="UI_AtW_ItemPurchased" button="UI_OK" />
		<Action type="focusOnMenuItem" itemName="DeathMountain" inventory="true" />
	</DrColossusBundle_Purchased>
</ScriptPackage>