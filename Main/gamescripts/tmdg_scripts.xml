	<Tappable_Bigclaw_Tapped ignoreHUD="true">
		<Action type="if">
			<If expression="greaterThan(TMDG_TappableBigclawTaps, 2)">
				<Then>
					<Action type="updateVariable" variable="TMDG_TappableBigclawTaps" value="2" />
				</Then>
			</If>
		</Action>
		<Action type="checkExpression" expression="TMDG_TappableBigclawTaps">
			<ExpressionCase value="0">
				<Action type="sound" sound="Bigclaw_Gobble" />
				<Action type="flashObject" object="Tappable_Bigclaw" color="FF00FFFF" period="0.38" instanceDuration="0.18" />
				<Action type="updateVariable" variable="TMDG_TappableBigclawTaps" value="TMDG_TappableBigclawTaps+1" />
				<Action type="runScript" script="TMDG_Scripts:Tappable_BigclawRun" />
			</ExpressionCase>
			<ExpressionCase value="1">
				<Action type="sound" sound="Bigclaw_Gobble" />
				<Action type="flashObject" object="Tappable_Bigclaw" color="FF00FFFF" period="0.25" instanceDuration="0.15" />
				<Action type="updateVariable" variable="TMDG_TappableBigclawTaps" value="TMDG_TappableBigclawTaps+1" />
				<Action type="runScript" script="TMDG_Scripts:Tappable_BigclawRun" />
			</ExpressionCase>
			<ExpressionCase value="2">
				<Action type="flashObject" object="Tappable_Bigclaw" remove="true" />
				<Action type="setVariable" dateVariable="BigclawStartTime" />
				<Action type="sound" sound="Bigclaw_Tap" />
				<Action type="playanimation" character="Tappable_Bigclaw" animation="Tap" />
				<Action type="setVariable" boolVariable="BigclawSpawned" boolValue="false" />
				<Action type="reward" TMDG_Pelts="__TMDG_GameConfig:CurrencyRewards:BigclawRewardBaseValue__" />
				<Action type="reward" TMDG_Pelts="__TMDG_GameConfig:CurrencyRewards:BigclawRewardBaseValue__" />
				<Action type="reward" TMDG_Pelts="__TMDG_GameConfig:CurrencyRewards:BigclawRewardBaseValue__" />
				<Action type="reward" TMDG_Pelts="__TMDG_GameConfig:CurrencyRewards:BigclawRewardBaseValue__" />
				<Action type="reward" TMDG_Pelts="__TMDG_GameConfig:CurrencyRewards:BigclawRewardBaseValue__" />
				<Action type="updateVariable" variable="TMDG_BigclawQueued" value="false" />
				<Action type="updateVariable" variable="TMDG_TappableBigclawTaps" value="TMDG_TappableBigclawTaps+1" />
				<Action type="runScript" script="TMDG_Scripts:RegisterTelemetry_Bigclaw" ignoreObject="true" />
				<Action type="deletecharacter" character="Tappable_Bigclaw" />
			</ExpressionCase>
		</Action>
	</Tappable_Bigclaw_Tapped>
