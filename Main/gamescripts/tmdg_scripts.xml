	<BigclawTapped parallel="true">
		<Action type="if">
			<If expression="time(0) &gt;= TMDG_BigclawCooldown">
				<Then>
					<Action type="updateVariable" variable="TMDG_BigclawTaps" value="0" />
				</Then>
			</If>
		</Action>
		<Action type="if">
			<If expression="TMDG_BigclawTaps==0">
				<Then>
					<Action type="updateVariable" variable="TMDG_BigclawCooldown" value="time(0)+(60*60*24)" />
				</Then>
			</If>
		</Action>
		<Action type="updateVariable" variable="TMDG_BigclawTaps" value="TMDG_BigclawTaps+1" />
		<Action type="if">
			<If expression="lessThan(TMDG_BigclawTaps,4)">
				<Then>
					<Action type="sound" sound="Bigclaw_Tap" />
					<Action type="anim-play" character="Bigclaw" animation="Tap" />
					<Action type="rewardchoice">
						<Reward chance="0.50" chanceType="s" money="250">
							<Requirement type="quest" quest="SpringfieldHeights08" not="true" />
						</Reward>
						<Reward chance="0.50" chanceType="s" Pharmaceutical="2">
							<Requirement type="quest" quest="SpringfieldHeights08" />
						</Reward>
						<Reward chance="0.75" chanceType="s" money="500" />
						<Reward chance="1.0" chanceType="s" xp="50" />
					</Action>
					<Action type="runScript" script="TMDG_Scripts:BigclawRun" />
				</Then>
			</If>
			<Else>
				<Then>
					<Action type="sound" sound="Bigclaw_Gobble" />
					<Action type="flashObject" object="Bigclaw" color="FF00FFFF" period="0.55" instanceDuration="0.2" />
					<Action type="delay" time="2" />
					<Action type="flashObject" object="Bigclaw" remove="true" />
				</Then>
			</Else>
		</Action>
	</BigclawTapped>
	<BigclawTapped_Cooldown parallel="true">
		<Action type="sound" sound="Bigclaw_Tap" />
		<Action type="anim-play" character="Bigclaw" animation="Tap" />
	</BigclawTapped_Cooldown>
	<BigclawRun parallel="true">
		<Action type="updateVariable" variable="TMDG_BigclawRun" value="true" />
		<Action type="delay" time="7" />
		<Action type="updateVariable" variable="TMDG_BigclawRun" value="false" />
	</BigclawRun>
	<Tappable_BigclawRun parallel="true">
