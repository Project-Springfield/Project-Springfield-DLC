		<Action type="delay" time="2" />
		<Action type="flashObject" object="Tappable_Bigclaw" remove="true" />
		<Action type="delay" time="5" />
		<Action type="updateVariable" variable="TMDG_Tappable_BigclawRun" value="false" />
	</Tappable_BigclawRun>
