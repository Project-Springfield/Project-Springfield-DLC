	</PlaneBot_Tapped>
	<Globe_Tapped>
		<Action type="anim-play" animation="ActiveNoLoop" waitforend="true" />
		<Action type="program">
			<Program><![CDATA[
					#program;

					var globe = Land::getFirstInstanceOfObject(Building::find('Globe'));
					var randVal = toDouble(Object::getVariable(globe, 'rand'));
					var newRand = rand(24);
					if(randVal!=newRand)
					{
						Object::setVariable(globe, 'rand', toString(newRand), 0, false);
					}
					else
					{
						Object::setVariable(globe, 'rand', toString(mod(newRand+1, 24)), 0, false);
					}
				]]></Program>
		</Action>
