	<OsakaSquidTapped parallel="true">
		<Action type="if">
			<If expression="!OsakaSquidTapped">
				<Then>
					<Action type="setVariable" boolVariable="OsakaSquidTapped" boolValue="true" />
					<Action type="setObjectVariable" onType="character" onObject="OsakaSquid" variable="Random" value="rand(100)" asExpression="true" />
					<Action type="if">
						<If expression="greaterThanEQ(System::evaluate('Character::OsakaSquid::Object::Random'), 50)">
							<Then>
								<Action type="anim-play" character="OsakaSquid" animation="Tap_1" />
							</Then>
						</If>
						<Else>
							<Then>
								<Action type="anim-play" character="OsakaSquid" animation="Tap_2" />
							</Then>
						</Else>
					</Action>
					<Action type="setVariable" boolVariable="OsakaSquidTapped" boolValue="false" />
				</Then>
			</If>
		</Action>
	</OsakaSquidTapped>
