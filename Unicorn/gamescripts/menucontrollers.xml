<MenuControllers>
	<Sequence name="PresentNeutral">
		<Animation name="PresentNeutral" />
	</Sequence>
	<Sequence name="Donut1Pick">
		<Animation name="Donut01Active" />
		<Animation name="Donut01Neutral" />
	</Sequence>
	<Sequence name="Donut1Inactive">
		<Animation name="Donut01Inactive" />
	</Sequence>
	<Sequence name="Donut2Pick">
		<Animation name="Donut02Active" />
		<Animation name="Donut02Neutral" />
	</Sequence>
	<Sequence name="Donut2Inactive">
		<Animation name="Donut02Inactive" />
	</Sequence>
	<Sequence name="Donut3Pick">
		<Animation name="Donut03Active" />
		<Animation name="Donut03Neutral" />
	</Sequence>
	<Sequence name="Donut3Neutral">
		<Animation name="Donut03Neutral" />
	</Sequence>
	<Sequence name="LightBegin">
		<Animation name="LightsAll" />
		<Loop loops="2">
			<Animation name="LightsAlternate" />
		</Loop>
		<Loop loops="3">
			<Animation name="LightsChase" />
		</Loop>
		<Animation name="LightsAll" />
		<Loop loops="2">
			<Animation name="LightsAlternate" />
		</Loop>
		<Animation name="LightsCascadeDown" />
		<Animation name="LightsCascadeUp" />
	</Sequence>
	<Sequence name="Prestige_BeginLights">
		<Loop>
			<Controller name="LightBegin" />
		</Loop>
	</Sequence>
	<Sequence name="LightInactive">
		<Animation name="LightsInactive" />
	</Sequence>
	<Sequence name="LightNeutral">
		<Animation name="LightsNeutral" />
	</Sequence>
	<Sequence name="LightPicked">
		<Animation name="LightsAll" />
		<Loop loops="5">
			<Animation name="LightsCorner" />
		</Loop>
		<Animation name="LightsAll" />
	</Sequence>
	<Sequence name="LightNotPicked">
		<Loop loops="2">
			<Animation name="LightsAlternate" />
		</Loop>
		<Animation name="LightsCascadeDown" />
		<Animation name="LightsCascadeUp" />
	</Sequence>
	<Sequence name="Prestige_LightNotPicked">
		<Loop>
			<Controller name="LightNotPicked" />
		</Loop>
	</Sequence>
	<Sequence name="LightFinalPicked">
		<Animation name="LightsAll" />
		<Animation name="LightsAll" />
		<Loop>
			<Animation name="LightsAlternate" />
		</Loop>
	</Sequence>
	<Sequence name="LightFinalNotPicked">
		<Animation name="LightsInactive" />
	</Sequence>
	<Sequence name="LightFinalNeverPicked">
		<Animation name="LightsNeutral" />
	</Sequence>
	<Sequence name="WheelIdle">
		<Animation name="Idle" loop="true" />
	</Sequence>
	<Sequence name="WheelSpinning">
		<Animation name="Spinning" loop="true" />
	</Sequence>
	<Sequence name="WheelVictory">
		<Animation name="Victory" loop="true" />
	</Sequence>
	<Sequence name="WheelDisabled">
		<Animation name="AllOff" loop="true" />
	</Sequence>
</MenuControllers>