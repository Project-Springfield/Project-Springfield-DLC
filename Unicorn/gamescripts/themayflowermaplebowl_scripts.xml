<ScriptPackage>
	<ShowEventIntro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="TheMayflowerMapleBowlMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="TheMayflowerMapleBowl_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="TheMayflowerMapleBowl_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="TheMayflowerMapleBowl_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="TheMayflowerMapleBowl_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="timeLeft">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_IntroPanelFooter" localise="true" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="textColumn1">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_IntroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn2">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_IntroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_TheMayflowerMapleBowl_MiniEventPanel_Intro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="setVariable" boolVariable="TheMayflowerMapleBowl_IntroSeen" boolValue="true" />
		<Action type="updateVariable" variable="TheMayflowerMapleBowl_Store" value="true" />
		<Action type="recommendation" recType="Event" name="TheMayflowerMapleBowl_TheMayflowerMapleBowl_OnePanel" eventName="UI_TheMayflowerMapleBowl_Title" limitedTime="true" limitedTimeEndDate="TheMayflowerMapleBowl_End" />
		<Action type="reparseBuildMenu" />
	</ShowEventIntro>
	<ShowEventOutro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="TheMayflowerMapleBowlMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="textColumn4">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_OutroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn5">
					<Attribute name="text" value="UI_TheMayflowerMapleBowl_OutroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_TheMayflowerMapleBowl_MiniEventPanel_Outro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="notificationLarge" title="CON_ShatteredDreamsMysteryBox_name" button="UI_OK" text="UI_ShatteredDreams_notif" icon="ICO_STOR_Single_ShatteredDreams_MysteryBox" />
		<Action type="reward" save="true" ShatteredToken="1" />
		<Action type="recommendation" recType="Promoted" name="ShatteredDreamsMysteryBox_Gaming" limitedTime="true" limitedTimeEndDate="TheMayflowerMapleBowl_ShatteredDreamsEnd" />
		<Action type="reparseBuildMenu" />
		<Action type="consumable" consumable="ShatteredDreamsMysteryBox_Gaming" fromInventory="false" />
	</ShowEventOutro>
	<ExpiryWarning_72hr>
		<Action type="message" text="UI_TheMayflowerMapleBowl_72HourExpiry_text" />
		<Action type="setVariable" boolVariable="TheMayflowerMapleBowl_PreExpiryDisplayed72" boolValue="true" />
		<Action type="runScript" script="TheMayflowerMapleBowl_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_72hr>
	<ExpiryWarning_24hr>
		<Action type="message" text="{FormattedExpiration24}" formatMe="root">
			<FormattingConfig path="TheMayflowerMapleBowl_GameConfig:ExpirationFormattedText" />
		</Action>
		<Action type="setVariable" boolVariable="TheMayflowerMapleBowl_PreExpiryDisplayed24" boolValue="true" />
		<Action type="runScript" script="TheMayflowerMapleBowl_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_24hr>
	<DisplayExpiry>
		<Action type="message" text="UI_TheMayflowerMapleBowl_Expired_text" />
		<Action type="clearRecommendations" category="Event" name="TheMayflowerMapleBowl_OnePanel" />
		<Action type="runScript" script="TheMayflowerMapleBowl_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</DisplayExpiry>
	<EventCompleted>
		<Action type="setVariable" boolVariable="TheMayflowerMapleBowl_Complete" boolValue="true" />
	</EventCompleted>
	<OpenMasterQuest>
		<Action type="checkRequirements">
			<Requirement type="gamestate" state="GameState_QuestProgress" />
			<True>
				<Action type="changeQuestForProgressState" name="TheMayflowerMapleBowl_MasterQuest" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_QuestProgress" forceState="true">
					<Extras>
						<Attribute name="questParentDelegate" value="TheMayflowerMapleBowl_MasterQuest" />
					</Extras>
				</Action>
			</False>
		</Action>
	</OpenMasterQuest>
	<ShowJobList ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="if">
					<If>
						<Requirement type="active quest" quest="ALARPInTheWoods_1" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="TheMayflowerMapleBowl_Event" />
									<Attribute name="JobList" value="TheMayflowerMapleBowl_JL_1" />
								</Extras>
							</Action>
						</Then>
					</If>
					<ElseIf>
						<Requirement type="active quest" quest="ALARPInTheWoods_2" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="TheMayflowerMapleBowl_Event" />
									<Attribute name="JobList" value="TheMayflowerMapleBowl_JL_2" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ALARPInTheWoods_3" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="TheMayflowerMapleBowl_Event" />
									<Attribute name="JobList" value="TheMayflowerMapleBowl_JL_3" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ALARPInTheWoods_4" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="TheMayflowerMapleBowl_Event" />
									<Attribute name="JobList" value="TheMayflowerMapleBowl_JL_4" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ALARPInTheWoods_5" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="TheMayflowerMapleBowl_Event" />
									<Attribute name="JobList" value="TheMayflowerMapleBowl_JL_5" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
				</Action>
			</True>
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</ShowJobList>
	<CheckIfEnd ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="quest" quest="TheMayflowerMapleBowl_EventExpired" />
			<True>
				<Action type="setVariable" boolVariable="TheMayflowerMapleBowl_Expired" boolValue="true" />
			</True>
		</Action>
	</CheckIfEnd>
	<AutoApply_PuritanLovejoySkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="TheMayflowerMapleBowl_Scripts:AutoApply_PuritanLovejoySkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_PuritanLovejoySkin_End>
	<AutoApply_PuritanLovejoySkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="PuritanLovejoy" fromInventory="true" />
		<Action type="pantoobject" character="Lovejoy" />
	</AutoApply_PuritanLovejoySkin_Delayed>
	<AutoApply_ScoutingLisaSkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="TheMayflowerMapleBowl_Scripts:AutoApply_ScoutingLisaSkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_ScoutingLisaSkin_End>
	<AutoApply_ScoutingLisaSkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="ScoutingLisa" fromInventory="true" />
	</AutoApply_ScoutingLisaSkin_Delayed>
	<AutoApply_ScoutingBartSkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="TheMayflowerMapleBowl_Scripts:AutoApply_ScoutingBartSkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_ScoutingBartSkin_End>
	<AutoApply_ScoutingBartSkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="ScoutingBart" fromInventory="true" />
	</AutoApply_ScoutingBartSkin_Delayed>
	<RecommendStoreCategory_TheMayflowerMapleBowl ignoreHUD="true">
		<Action type="reparseBuildMenu" />
		<Action type="clearRecommendations" category="Event" name="TheMayflowerMapleBowl_TheMayflowerMapleBowl_OnePanel" />
		<Action type="recommendation" recType="Event" name="TheMayflowerMapleBowl_TheMayflowerMapleBowl_OnePanel" eventName="UI_TheMayflowerMapleBowl_Title" limitedTime="true" limitedTimeEndDate="TheMayflowerMapleBowl_End" />
		<Action type="focusOnMenuItem" menuCategory="FrontPage" />
		<Action type="reparseBuildMenu" />
	</RecommendStoreCategory_TheMayflowerMapleBowl>
	<EnterStoreCategory_TheMayflowerMapleBowl ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="TheMayflowerMapleBowl_Category" />
	</EnterStoreCategory_TheMayflowerMapleBowl>
	<TheMayflowerMapleBowl_MTX_DisplayPromo ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="Currency" itemName="TheMayflowerMapleBowl_TrayOfDonuts" />
	</TheMayflowerMapleBowl_MTX_DisplayPromo>
	<GoToPilgrimBundle>
		<Action type="focusOnMenuItem" menuCategory="TheMayflowerMapleBowl_Category" itemName="PilgrimLARPGOB" />
	</GoToPilgrimBundle>
	<PuritanLovejoyReward>
		<Action type="checkRequirements">
			<Requirement type="character" name="Lovejoy" not="true" />
			<True>
				<Action type="rewardWithPlaceStore" attributes="Config_MiniEvents:RewardWithPlaceStore">
					<Item type="consumable" name="PuritanLovejoy" useImmediately="true" />
				</Action>
			</True>
			<False>
				<Action type="reward">
					<Item type="consumable" name="PuritanLovejoy" useImmediately="true" />
				</Action>
			</False>
		</Action>
	</PuritanLovejoyReward>
</ScriptPackage>