	<FireDeptJob_MenuConfig_HomerBusy>
		<Merge path="CrookAndLadder_MenuConfig:FireDeptJob_MenuConfig_BusyCommon" />
		<Attribute name="MinigameWithIcon">
			<AttributeSet name="ReadyIcon">
				<Attribute name="image" program="program">
					<Program><![CDATA[
                            #program;
							var homer = Character::find('Homer');
                            var character = Land::getFirstInstanceOfObject(homer);
							var proxy = Land::getFirstProxyInstanceOfObject(homer);
                            if(!isNULL(character))
                            {
                                return Object::getProperty(character, 'MenuAvatar');
                            }
							elseif (!isNULL(proxy))
							{
								return Object::getProperty(proxy, 'MenuAvatar');
							}
                            return '';
                        ]]></Program>
