<Attributes>
	<CharacterJobDefaults>
		<Attribute name="Enabled" value="true" />
		<Attribute name="EnableForRewards" value="Money|XP|Donuts" />
		<Attribute name="HideButton" value="false" />
		<Attribute name="Cost" value="{RushRules::RushCost}" formatMe="true" />
		<Attribute name="Cost" value="0" formatMe="true">
			<Requirements>
				<Requirement type="formula" formula="lessThanEQ({Object::RemainingTime}, min(7200, 0.1*{Object::TotalTime}))" />
			</Requirements>
		</Attribute>
	</CharacterJobDefaults>
	<CharacterJobNoFreeRush>
		<Attribute name="Enabled" value="true" />
		<Attribute name="EnableForRewards" value="Money|XP|Donuts" />
		<Attribute name="HideButton" value="false" />
		<Attribute name="Cost" value="{RushRules::RushCost}" formatMe="true" />
	</CharacterJobNoFreeRush>
	<TaxJobDefaults>
		<Attribute name="Enabled" value="true" />
		<Attribute name="EnableForRewards" value="Money|XP|Donuts" />
		<Attribute name="HideButton" value="true" />
	</TaxJobDefaults>
	<FarmJobDefaults>
		<Attribute name="Enabled" value="false" />
	</FarmJobDefaults>
	<CasinoJobDefaults>
		<Attribute name="Enabled" value="false" />
	</CasinoJobDefaults>
	<TaxJobNoRush>
		<Attribute name="Enabled" value="true" />
		<Attribute name="HideButton" value="true" />
	</TaxJobNoRush>
</Attributes>