			<ExpressionCase value="1">
				<Action type="SilentlyUnlockSkin" skin="Monument_Stage2" />
			</ExpressionCase>
			<ExpressionCase value="2">
				<Action type="SilentlyUnlockSkin" skin="Monument_Stage3" />
