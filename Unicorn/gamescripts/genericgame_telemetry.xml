<TelemetryFamilies>
	<GenericGame>
		<TriggerEffects>
			<TriggerEffect>
				<Filter type="matching" key="TriggerType" string="OnPrizeboxClaimed" />
				<Response type="logEvent">
					<Event id="4041">
						<Parameter0 type="Enumeration" source="FixedString" extra="{Trigger::Source}" />
						<Parameter1 type="Enumeration" source="FormulaAsInt" extra="{Trigger::Amount}" />
						<Parameter2 type="Enumeration" source="FixedString" extra="{Trigger::ObjectName}" />
					</Event>
				</Response>
			</TriggerEffect>
			<TriggerEffect>
				<Filter type="matching" key="TriggerType" string="OnInfoButtonPressed" />
				<Response type="logEvent">
					<Event id="4042">
						<Parameter0 type="Enumeration" source="FixedString" extra="{Trigger::ObjectName}" />
						<Parameter1 type="Enumeration" source="FixedString" extra="{Trigger::Source}" />
						<Parameter2 type="UserLevel" source="UserLevel" />
					</Event>
				</Response>
			</TriggerEffect>
		</TriggerEffects>
	</GenericGame>
</TelemetryFamilies>