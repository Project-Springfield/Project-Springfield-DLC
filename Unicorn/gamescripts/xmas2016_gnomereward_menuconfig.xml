<Attributes>
	<MenuConfig>
		<Attribute name="Objects">
			<AttributeSet name="IfGnomeOut">
				<Attribute name="active" formula="lessThanEQ(ENDTIME, serverTime(0))" />
			</AttributeSet>
			<AttributeSet name="HasGuide">
				<Attribute name="active" value="true" />
			</AttributeSet>
			<AttributeSet name="MenuTitleText">
				<Requirement type="formula" formula="!RewardsRemaining" />
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_title" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="MenuTitleText">
				<Requirement type="formula" formula="RewardsRemaining" />
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_title_failure" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="SummaryText1">
				<Requirement type="formula" formula="!RewardsRemaining" />
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_text" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="SummaryText1">
				<Requirement type="formula" formula="RewardsRemaining" />
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_text_failure" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="SummaryText2">
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_Loot_text" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="color" value="0xFFD51AFF" />
			</AttributeSet>
			<AttributeSet name="SummaryText3">
				<Requirement type="variable" variable="GnomeGameSocialMenu" not="true" />
				<Attribute name="text" value="UI_XMAS2016_GnomeReward_Cooldown_text" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="textReplacements">
					<AttributeSet>
						<Attribute name="index" value="0" />
						<Attribute name="text" formula="GnomeCooldownTime / 3600" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="GenericGraphic">
				<Attribute name="image" value="ICO_XMAS2016_GnomeInYourHome_RewardPopup_Gnome" />
			</AttributeSet>
			<AttributeSet name="OKButtonText">
				<Attribute name="text" value="UI_Collect" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
			</AttributeSet>
			<AttributeSet name="OKButtonTextHL">
				<Attribute name="text" value="UI_Collect" localise="true" />
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="color" value="000000FF" />
			</AttributeSet>
			<AttributeSet name="RewardText">
				<Attribute name="iconText">
					<AttributeSet>
						<Requirement type="formula" formula="GnomeGuiltDustPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_guiltdust_sm" />
						<Attribute name="text" formula="GnomeGuiltDustRewarded" />
					</AttributeSet>
					<AttributeSet>
						<Requirement type="formula" formula="GnomeGiftReceiptPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_giftreceipt_sm" />
						<Attribute name="text" formula="GnomeGiftReceiptRewarded" />
					</AttributeSet>
					<AttributeSet>
						<Requirement type="formula" formula="GnomeCatnipPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_catnip_sm" />
						<Attribute name="text" formula="GnomeCatnipRewarded" />
					</AttributeSet>
					<AttributeSet>
						<Requirement type="formula" formula="GnomeCarvedFigurinesPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_carvedfigurine_sm" />
						<Attribute name="text" formula="GnomeCarvedFigurinesRewarded" />
					</AttributeSet>
					<AttributeSet>
						<Requirement type="formula" formula="GnomeBookOfShadowsPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_bookofshadows_sm" />
						<Attribute name="text" formula="GnomeBookOfShadowsRewarded" />
					</AttributeSet>
					<AttributeSet>
						<Requirement type="formula" formula="GnomeChickenFingersPerMS" />
						<Attribute name="icon" value="ico_cur_xmas2016_chickenfingers_sm" />
						<Attribute name="text" formula="GnomeChickenFingersRewarded" />
					</AttributeSet>
				</Attribute>
				<Attribute name="font" value="eFont_SmallBold" />
				<Attribute name="color" value="FFFFFFFF" />
			</AttributeSet>
		</Attribute>
	</MenuConfig>
	<ButtonsConfig>
		<Attribute name="Buttons">
			<AttributeSet name="OKButton">
				<Attribute name="script" value="MiscScripts:GoToInGameWithSound" />
			</AttributeSet>
		</Attribute>
	</ButtonsConfig>
</Attributes>