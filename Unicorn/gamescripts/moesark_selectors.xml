<DynamicDataSelectors>
	<Selector name="MoesArk_PTCurrencyPerFriendAction" type="table" key="FriendActionsCount">
		0,selector MoesArk_PTCurrency_SocialUpgrade,
		30,0,
	</Selector>
	<Selector name="MoesArk_PTCurrency_SocialUpgrade" type="table" key="MoesArk_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__MoesArk_GameConfig:TappablePrizeReward:FriendAmount__" />
		<TableEntry key="1" value="2 * toDouble(System::getConfigValue('__MoesArk_GameConfig:TappablePrizeReward:FriendAmount__'))" />
	</Selector>
	<Selector name="MoesArk_CraftingCurrencyPerFriendAction" type="table" key="FriendActionsCount">
		0,selector MoesArk_CraftingCurrency_SocialUpgrade,
		30,0,
	</Selector>
	<Selector name="MoesArk_CraftingCurrency_SocialUpgrade" type="table" key="MoesArk_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__MoesArk_GameConfig:TappableCraftReward:FriendAmount__" />
		<TableEntry key="1" value="2 * toDouble(System::getConfigValue('__MoesArk_GameConfig:TappableCraftReward:FriendAmount__'))" />
	</Selector>
	<Selector name="MoesArk_SocialCurrencyPerFriendAction" type="table" key="FriendActionsCount">
		0,selector MoesArk_SocialCurrency_SocialUpgrade,
		30,0,
	</Selector>
	<Selector name="MoesArk_SocialCurrency_SocialUpgrade" type="table" key="MoesArk_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__MoesArk_BalanceConfig:Tappables:Friend_Award_Social1__" />
		<TableEntry key="1" value="2 * toDouble(System::getConfigValue('__MoesArk_BalanceConfig:Tappables:Friend_Award_Social1__'))" />
	</Selector>
</DynamicDataSelectors>