<ScriptPackage>
	<RocketLaunchPlatform_JobStarted parallel="true" otherScriptsCanRunOnObject="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="quest" quest="SpaceTrek" />
			</Requirements>
			<False>
				<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="Failures" value="0" />
				<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="LaunchedState" value="1" />
			</False>
			<True>
				<Action type="if">
					<If expression="and(or(greaterThan(System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Failures'), 2), rand(4)==0), System::evaluate('Building::RocketLaunchPlatform_Farm::Object::LaunchedState')!=2)">
						<Then>
							<Action type="sound" sound="bld_RocketLaunchPlatform_Ignition" />
							<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="Failures" value="0" />
							<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="LaunchedState" value="1" />
						</Then>
					</If>
					<Else>
						<Then>
							<Action type="sound" sound="bld_RocketLaunchPlatform_Crash" />
							<Action type="if">
								<If expression="System::evaluate('Building::RocketLaunchPlatform_Farm::Object::LaunchedState')!=2">
									<Then>
										<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="Failures" value="System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Failures')+1" asExpression="true" />
									</Then>
								</If>
							</Action>
							<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="LaunchedState" value="2" />
							<Action type="delay" time="5" />
							<Action type="setVariable" boolVariable="RocketFailed" boolValue="true" />
						</Then>
					</Else>
				</Action>
			</True>
		</Action>
	</RocketLaunchPlatform_JobStarted>
	<RocketLaunchPlatform_JobComplete parallel="true" otherScriptsCanRunOnObject="true">
		<Action type="if">
			<If expression="System::evaluate('Building::RocketLaunchPlatform_Farm::Object::LaunchedState')==1">
				<Then>
					<Action type="anim-play" building="RocketLaunchPlatform_Farm" animation="Active_2_Out" />
					<Action type="checkRequirements">
						<Requirement type="quest" quest="SpaceTrek" />
						<True>
							<Action type="runScript" script="RocketLaunch_Success" package="DSH_Scripts" />
						</True>
					</Action>
				</Then>
			</If>
			<Else>
				<Then>
					<Action type="anim-play" building="RocketLaunchPlatform_Farm" animation="ACTIVE_3_Out" />
				</Then>
			</Else>
		</Action>
		<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="LaunchedState" value="0" />
		<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="InPlayed" value="0" />
	</RocketLaunchPlatform_JobComplete>
	<RocketLaunch_Success otherScriptsCanRunOnObject="true">
		<Action type="setObjectVariable" onType="building" onObject="RocketLaunchPlatform_Farm" variable="Random" value="rand(100)" asExpression="true" />
		<Action type="if">
			<If expression="System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Random')==0">
				<Then>
					<Action type="rewardWithPlaceStore" attributes="DSH_MenuConfig:RewardWithPlaceStore">
						<Item type="building" name="MiniNuclearWarhead" />
					</Action>
				</Then>
			</If>
			<ElseIf expression="lessThan(System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Random'), 4)">
				<Then>
					<Action type="rewardWithPlaceStore" attributes="DSH_MenuConfig:RewardWithPlaceStore">
						<Item type="building" name="KangTopiary" />
					</Action>
				</Then>
			</ElseIf>
			<ElseIf expression="lessThan(System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Random'), 7)">
				<Then>
					<Action type="rewardWithPlaceStore" attributes="DSH_MenuConfig:RewardWithPlaceStore">
						<Item type="building" name="KodosTopiary" />
					</Action>
				</Then>
			</ElseIf>
			<ElseIf expression="lessThan(System::evaluate('Building::RocketLaunchPlatform_Farm::Object::Random'), 40)">
				<Then>
					<Action type="rewardWithPlaceStore" attributes="DSH_MenuConfig:RewardWithPlaceStore">
						<Item type="building" name="HailAntsSign" />
					</Action>
				</Then>
			</ElseIf>
			<Else>
				<Then>
					<Action type="reward" ignoreObject="true">
						<Item type="chance">
							<Item type="building" name="Debris_00" chance="0.2" />
							<Item type="building" name="Debris_01" chance="0.4" />
							<Item type="building" name="Debris_02" chance="0.6" />
							<Item type="building" name="Debris_03" chance="0.8" />
							<Item type="building" name="Debris_04" chance="1" />
						</Item>
						<Item type="chance">
							<Item type="building" name="Debris_00" chance="0.2" />
							<Item type="building" name="Debris_01" chance="0.4" />
							<Item type="building" name="Debris_02" chance="0.6" />
							<Item type="building" name="Debris_03" chance="0.8" />
							<Item type="building" name="Debris_04" chance="1" />
						</Item>
						<Item type="chance">
							<Item type="building" name="Debris_00" chance="0.2" />
							<Item type="building" name="Debris_01" chance="0.4" />
							<Item type="building" name="Debris_02" chance="0.6" />
							<Item type="building" name="Debris_03" chance="0.8" />
							<Item type="building" name="Debris_04" chance="1" />
						</Item>
						<Item type="chance">
							<Item type="building" name="Debris_00" chance="0.2" />
							<Item type="building" name="Debris_01" chance="0.4" />
							<Item type="building" name="Debris_02" chance="0.6" />
							<Item type="building" name="Debris_03" chance="0.8" />
							<Item type="building" name="Debris_04" chance="1" />
						</Item>
					</Action>
					<Action type="notification" title="BLD_Debris_00_name" button="UI_OK" text="UI_DSH_DebrisRewarded" icon="Debris00_Menu" />
				</Then>
			</Else>
		</Action>
	</RocketLaunch_Success>
