		<GridType type="grass|pavement|boardwalk|boardwalkedge|pier|pieredge|dirtpavement|klsroad|klsroadedge" />
		<RatingElem service="100" decoration="1000" restaurant="100" housing="150" shopping="100" environment="500" crime="10" community="150" />
		<BSv2 name="KeyToTheCityStatue" />
		<SoundKit event="BuildingTouched" name="KeyToTheCity_Tap" />
		<OnEvent event="BuildingTouched" building="KeyToTheCity">
			<Script name="DecorationTapped_SingleTap_Wait" package="MiscScripts" />
		</OnEvent>
	</Building>
