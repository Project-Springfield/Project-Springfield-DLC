	<Building id="129500" name="XPCollider" type="AnimatedDecoration" xpValue="100">
		<Requirements>
			<Requirement type="level" level="20" />
		</Requirements>
		<VisibilityRequirements>
			<Requirement type="level" level="20" />
			<Requirement type="formula" formula="System::evaluate('Powerup::Master::Enabled')" hideFromInfo="true" showWhenLockedItemDesc="false" />
			<Requirement type="formula" formula="System::evaluate('Powerup::XPBonus::Enabled')" hideFromInfo="true" showWhenLockedItemDesc="false" />
		</VisibilityRequirements>
		<InventoryRequirements>
			<Requirement type="level" level="20" />
		</InventoryRequirements>
		<Unique value="true" />
		<Sell allowed="false" storeoverride="true" />
		<CustomCantStoreMessage value="UI_Powerups_CantStoreXPCollider" />
		<CanStoreRequirements>
			<Requirement type="formula" formula="lessThanEQ(System::evaluate('Powerup::XPBonus::TimeLeft::Seconds'), 0)" />
		</CanStoreRequirements>
		<BuildTime time="0" />
		<GridType type="grass|dirtpavement|pavement" />
		<NeutralAnimationController name="XPColliderChargeCheck" />
		<OnEvent event="BuildingTouched" user="local">
			<Requirements>
				<Requirement type="formula" formula="System::evaluate('Powerup::Master::Enabled')" />
				<Requirement type="formula" formula="System::evaluate('Powerup::XPBonus::Enabled')" />
			</Requirements>
			<Script name="ShowXPColliderMenu" package="NewUserPowerUps_PowerupScripts" />
		</OnEvent>
		<Sound event="BuildingTouched" sound="sfx_Collider_Tap.caf" />
	</Building>
