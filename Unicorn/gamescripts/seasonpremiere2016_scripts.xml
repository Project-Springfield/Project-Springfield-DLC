	<MoesTeleport_JobDone parallel="true">
		<Action type="checkRequirements">
			<Requirement type="building" building="MoesTavern" />
			<True>
				<Action type="stop" />
				<Action type="if">
					<If expression="!System::evaluate('Building::MoesTavern::Flip::AsBool')">
						<Then>
							<Action type="moveObjectInstance" xPos="gridToWorld(System::evaluate('Building::MoesTavern::Object::GridX')+2.5)" yPos="gridToWorld(System::evaluate('Building::MoesTavern::Object::GridY')+1)" />
						</Then>
					</If>
					<Else>
						<Then>
							<Action type="moveObjectInstance" xPos="gridToWorld(System::evaluate('Building::MoesTavern::Object::GridX')-1)" yPos="gridToWorld(System::evaluate('Building::MoesTavern::Object::GridY')-2.5)" />
						</Then>
					</Else>
				</Action>
			</True>
		</Action>
	</MoesTeleport_JobDone>
	<MoesTeleport_Triggered parallel="true">
		<Action type="checkRequirements">
			<Requirement type="building" building="MoesTavern" />
			<True>
				<Action type="checkRequirements">
					<Requirement type="players land" />
					<True>
						<Action type="gotoState" state="GameState_InGame" />
					</True>
					<False>
						<Action type="gotoState" state="FriendState_InGame" parentState="ActiveState" />
					</False>
				</Action>
				<Action type="pantoobject" building="MoesTavern" fakeTouch="false" maxSpeed="1000" time="0.1" />
			</True>
			<False>
				<Action type="if">
					<If>
						<Requirement type="players land" />
						<Then>
							<Action type="checkRequirements">
								<Requirement type="item" item="MoesTavern" />
								<True>
									<Action type="focusOnMenuItem" itemName="MoesTavern" inventory="true" />
								</True>
								<False>
									<Action type="checkRequirements">
										<Requirement type="place building" building="MoesTavern" />
										<True>
											<Action type="returnToInGameState" />
											<Action type="pantoobject" building="MoesTavern" fakeTouch="false" maxSpeed="1000" time="0.1" />
										</True>
										<False>
											<Action type="focusOnMenuItem" itemName="MoesTavern" menuCategory="Characters" />
										</False>
									</Action>
								</False>
							</Action>
						</Then>
					</If>
					<Else>
						<Then>
							<Action type="returnToInGameState" />
						</Then>
					</Else>
				</Action>
			</False>
		</Action>
	</MoesTeleport_Triggered>
	<NewLardLad_Tapped parallel="true">
		<Action type="actionSequence" script="MiscScripts:ReturnToGameState" />
		<Action type="checkRequirements">
			<Requirement type="building" building="NewLardLad" skin="NewLardLad_Statue" />
			<True>
				<Action type="checkExpression" expression="Object::State">
					<ExpressionCase value="0">
						<Action type="anim-play" building="NewLardLad" animation="Active_In" />
					</ExpressionCase>
					<ExpressionCase value="1">
						<Action type="anim-play" building="NewLardLad" animation="Active_Out" />
					</ExpressionCase>
				</Action>
				<Action type="setObjectVariable" variable="State" value="mod(Object::State+1, 2)" asExpression="true" allowOrphan="false" />
			</True>
			<False>
				<Action type="setObjectVariable" variable="State" value="0" asExpression="true" allowOrphan="false" />
			</False>
		</Action>
