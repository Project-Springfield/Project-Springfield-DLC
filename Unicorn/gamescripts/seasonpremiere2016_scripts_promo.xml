<ScriptPackage>
	<SP_GoToCategory ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="SeasonPremiere2016_Promo" />
	</SP_GoToCategory>
	<SP_DisplayPromo>
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
			<True>
				<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_DisplayPromo_StartDialogue" />
				<Action type="recommendation" recType="Event" eventName="UI_CorporateKickbacks_name" name="SP2016_Promo_OnePanel" />
				<Action type="gotoState" state="GameState_InGame" />
				<Action type="displayPromo" event="SeasonPremiere2016_Main" />
				<Action type="runScript" script="KillGil" package="MiscScripts" />
				<Action type="setVariable" boolVariable="SeasonPremiere2016_PromoActive" boolValue="true" />
			</True>
		</Action>
	</SP_DisplayPromo>
	<SP_DisplayPromo_StartDialogue>
		<Action type="dialogue" character="Quimby" text="QST_SeasonPremierePromoIntro_3_dlg_Start_1_Quimby" revealSpeed="medium" emotion="angry" />
		<Action type="dialogue" character="Burns" text="QST_SeasonPremierePromoIntro_3_dlg_Start_2_Burns" revealSpeed="medium" emotion="diabolical" />
		<Action type="dialogue" character="Lisa" text="QST_SeasonPremierePromoIntro_3_dlg_Start_3_Lisa" revealSpeed="medium" emotion="shouting" />
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoIntro_3_dlg_Start_4_Gil" revealSpeed="medium" emotion="selling" />
		<Action type="checkRequirements">
			<Requirement type="time" end="SeasonPremiere2016_PromoSwitch" />
			<True>
				<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_DisplayPromo_StartDialogue_Wave1" />
				<Action type="updateVariable" variable="SeasonPremiere2016_PromoWave1Seen" value="1" />
			</True>
			<False>
				<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_DisplayPromo_StartDialogue_Wave2" />
				<Action type="updateVariable" variable="SeasonPremiere2016_PromoWave2Seen" value="1" />
			</False>
		</Action>
	</SP_DisplayPromo_StartDialogue>
	<SP_DisplayPromo_StartDialogue_Wave1>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave1_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="happy" />
		<Action type="dialogue" character="Quimby" text="QST_SeasonPremierePromoWave1_3_dlg_Start_2_Quimby" revealSpeed="medium" />
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave1_3_dlg_Start_3_Gil" revealSpeed="medium" emotion="selling" />
		<Action type="dialogue" character="Quimby" text="QST_SeasonPremierePromoWave1_3_dlg_Start_4_Quimby" revealSpeed="medium" emotion="happy" />
	</SP_DisplayPromo_StartDialogue_Wave1>
	<SP_DisplayPromo_StartDialogue_Wave2>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave2_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="happy" />
		<Action type="dialogue" character="Skinner" text="QST_SeasonPremierePromoWave2_3_dlg_Start_2_Skinner" revealSpeed="medium" />
		<Action type="checkRequirements">
			<Requirement type="variable" variable="SeasonPremiere2016_PromoWave1Seen" />
			<True>
				<Action type="updateVariable" variable="SeasonPremiere2016_PromoWave2Seen" value="1" />
				<Action type="checkRequirements">
					<Requirement type="variable" variable="SeasonPremiere2016_PromoItemPurchased" />
					<True>
						<Action type="updateVariable" variable="SeasonPremiere2016_PromoItemPurchased" value="0" />
					</True>
				</Action>
				<Action type="displayPromo" event="SeasonPremiere2016_Main" />
			</True>
		</Action>
	</SP_DisplayPromo_StartDialogue_Wave2>
	<SP_DeclinePromo>
		<Action type="checkRequirements">
			<Requirement type="variable" variable="SeasonPremiere2016_PromoItemPurchased" not="true" />
			<True>
				<Action type="checkRequirements">
					<Requirement type="time" end="SeasonPremiere2016_PromoSwitch" />
					<True>
						<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_DeclinePromo_Wave1" />
					</True>
					<False>
						<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_DeclinePromo_Wave2" />
					</False>
				</Action>
			</True>
		</Action>
	</SP_DeclinePromo>
	<SP_DeclinePromo_Wave1>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave1Declined_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="surprised" />
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave1Declined_3_dlg_Start_2_Gil" revealSpeed="medium" emotion="sad" />
	</SP_DeclinePromo_Wave1>
	<SP_DeclinePromo_Wave2>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave2Declined_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="annoyed" />
	</SP_DeclinePromo_Wave2>
	<SP_PurchasePromo>
		<Action type="checkRequirements">
			<Requirement type="time" end="SeasonPremiere2016_PromoSwitch" />
			<True>
				<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_PurchasePromo_Wave1" />
			</True>
			<False>
				<Action type="actionSequence" script="SeasonPremiere2016_Scripts_Promo:SP_PurchasePromo_Wave2" />
			</False>
		</Action>
	</SP_PurchasePromo>
	<SP_PurchasePromo_Wave1>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave1Purchased_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="happy" />
	</SP_PurchasePromo_Wave1>
	<SP_PurchasePromo_Wave2>
		<Action type="dialogue" character="Gil" text="QST_SeasonPremierePromoWave2Purchased_3_dlg_Start_1_Gil" revealSpeed="medium" emotion="happy" />
	</SP_PurchasePromo_Wave2>
