	<Lair_Touched>
		<Attribute name="Kearneymon_LairTapped" program="program">
			<Program><![CDATA[
                    #program;
                    var name = ObjectVars::getVariableString('Obj_KearneymonCharacterJobs', '0');
                    if (name != 0)
                    {
                    var character = Land::getFirstInstanceOfObject(Character::find(name));
                    if(!isNULL(character))
                    {
                        var currentJob = Character::getCurrentJobInstance(character);
                        if(!isNULL(currentJob))
                        {
                            var jobID = Job::getProperty(currentJob, 'ID');
                            if(jobID == '184925')
                            {
                                Script::run('THOH2017_Scripts:EmulateTouched_'+name);
                                return 0;
                            }
                        }
                    }
                    }
                    return 1;
                ]]></Program>
