	<ChiliadRewardMenuConfig>
		<Attribute name="Objects">
			<AttributeSet name="NotificationTitleText">
				<Attribute name="text" value="UI_Chiliad_Minigame_RewardMenu_name" localise="true" />
			</AttributeSet>
			<AttributeSet name="Graphic">
				<Attribute name="image" value="ICO_RewardPopUp_SpaceCoyote" />
			</AttributeSet>
			<AttributeSet name="MessageContentText">
				<Attribute name="text" value="UI_Chiliad_Minigame_RewardMenu_desc" localise="true" />
			</AttributeSet>
			<AttributeSet name="Reward">
				<Attribute>
					<Attribute name="text" value=" {0}  {1}     {2}   {3} " />
					<Attribute name="iconReplacements">
						<AttributeSet>
							<Attribute name="index" value="0" />
							<Attribute name="icon" value="ico_rewardicon_money.rgb" />
							<Attribute name="icon" value="ico_heightsreward_pharmaceutical_sm.rgb">
								<Requirements>
									<Requirement type="quest" quest="SpringfieldHeights08" />
									<Requirement type="variable" variable="Insanity_CanHoldPharmaceuticals" />
								</Requirements>
							</Attribute>
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="2" />
							<Attribute name="icon" value="ico_rewardicon_xp.rgb" />
						</AttributeSet>
					</Attribute>
					<Attribute name="textReplacements">
						<AttributeSet>
							<Attribute name="index" value="1" />
							<Attribute name="text" formula="min((500+(DUDESCAUGHT*10)), 2000)" />
							<Attribute name="text" formula="min((1+floor(DUDESCAUGHT/7)), 5)">
								<Requirements>
									<Requirement type="quest" quest="SpringfieldHeights08" />
									<Requirement type="variable" variable="Insanity_CanHoldPharmaceuticals" />
									<Requirement type="formula" formula="HeightsMultiplierActive || toDouble(System::getConfigValue('__Multipliers_GameConfig:SpringfieldHeightsMultiplier:MultiplierActive__'))" not="true" />
								</Requirements>
							</Attribute>
							<Attribute name="text" formula="(min((1+floor(DUDESCAUGHT/7)), 5) * toDouble(System::getConfigValue('__Multipliers_GameConfig:SpringfieldHeightsMultiplier:Multiplier__')))">
								<Requirements>
									<Requirement type="quest" quest="SpringfieldHeights08" />
									<Requirement type="variable" variable="Insanity_CanHoldPharmaceuticals" />
									<Requirement type="formula" formula="HeightsMultiplierActive || toDouble(System::getConfigValue('__Multipliers_GameConfig:SpringfieldHeightsMultiplier:MultiplierActive__'))" />
								</Requirements>
							</Attribute>
						</AttributeSet>
						<AttributeSet>
							<Attribute name="index" value="3" />
							<Attribute name="text" formula="min((100+(DUDESCAUGHT*2)), 400)" />
						</AttributeSet>
					</Attribute>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="CollectText">
				<Attribute name="text" value="UI_Collect" localise="true" />
				<Attribute name="font" value="eFont_Small" />
			</AttributeSet>
			<AttributeSet name="CollectTextHL">
				<Attribute name="text" value="UI_Collect" localise="true" />
				<Attribute name="font" value="eFont_Small" />
			</AttributeSet>
		</Attribute>
