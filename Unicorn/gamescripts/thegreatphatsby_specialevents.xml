<SpecialEvents>
	<MasterList>
		<DataID id="171000" name="TheGreatPhatsbyTeaser" status="release" onDeprecated="delete" endDate="TheGreatPhatsbyTeaser_End" />
		<DataID id="171001" name="JayGPromo" status="release" onDeprecated="delete" endDate="JayGPromo_End" />
	</MasterList>
	<SpecialEvent id="171000" name="TheGreatPhatsbyTeaser">
		<Variable name="TheGreatPhatsby_Active" value="true" />
		<Jobs override="true">
			<Job name="VisitTheSummerMansion">
				<Requirement type="active quest" quest="MonogramMonty" />
			</Job>
		</Jobs>
	</SpecialEvent>
	<SpecialEvent id="171001" name="JayGPromo">
		<Variable name="JayG_PromoActive" value="true" />
		<Variable name="JayG_PromoIndicatorActive" value="true" />
		<Variable name="JayG_PromoShowInBuildMenu" value="false" />
		<Variable name="JayG_PromoSeen" value="false" />
		<Characters override="true">
			<Character name="PromoGilPermanent">
				<OnEvent event="CharacterTouched" user="local">
					<Requirement type="variable" variable="JayG_PromoIndicatorActive" />
					<Script name="JayGPromo_Display" package="TheGreatPhatsby_Scripts_Promo" />
				</OnEvent>
			</Character>
		</Characters>
		<Promo eventName="JayGPromo" questID="171050" type="building" object="JayGsMansion" file="Promo_TheGreatPhatsbyPromo" yes="BTN_225Donuts" visualEndDate="JayGPromo_End" />
	</SpecialEvent>
</SpecialEvents>