<Attributes>
	<SpookyCampfireLevelUpMenu>
		<Attribute name="enabled" value="true" />
		<Attribute>
			<Requirement type="players land" not="true" />
			<Attribute name="enabled" value="false" />
		</Attribute>
		<Attribute name="displayOnly" value="true" />
		<Attribute name="gotoUpgradeMenu" value="false" />
		<Attribute name="enableUpgrade" value="false" />
		<Attribute name="currentLevel" formula="Object::SpookyCampfireLevel+1" />
		<Attribute name="useObjectVariable" value="Object::SpookyCampfireLevel+1" />
		<Attribute name="gotoUpgradeMenuTitleText" value="UI_Level" localise="true" parameters="levelTextLine1">
			<AttributeSet name="levelTextLine1">
				<Attribute name="0" pointer="currentLevel" />
			</AttributeSet>
		</Attribute>
		<Attribute name="levelTextLine1" value="UI_CampfireLevel_text" localise="true" parameters="levelTextLine1">
			<AttributeSet name="levelTextLine1">
				<Attribute name="0" pointer="currentLevel" />
			</AttributeSet>
		</Attribute>
		<Attribute name="gotoUpgradeMenuTitleTextColor" value="f6ae00FF" />
		<Attribute name="gotoUpgradeMenuButtonText" value="UI_UpgradeButton_Name" localise="true" />
		<Attribute name="gotoUpgradeMenuDescText" value="UI_THOH2015_CampfireNothingSpawning" localise="true" />
		<Attribute name="gotoUpgradeMenuUpgradeReadyText" value="UI_THOH2015_CampfireNothingSpawning" localise="true" />
		<Attribute name="gotoUpgradeMenuButtonColor" value="0091F6FF" />
		<Attribute name="gotoUpgradeMenuButtonScript" value="THOH2015_Scripts:TabPressed_UpgradeMenu" />
		<Attribute>
			<Cost money="1" />
		</Attribute>
		<Attribute name="upgradeGotoMenuIcon" value="ICO_MHUD_THOH2015_Store.rgb" />
	</SpookyCampfireLevelUpMenu>
</Attributes>