<ScriptPackage>
	<ShowEventIntro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="MomsOfSpringfieldMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="RealMomsOfSpringfield_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="RealMomsOfSpringfield_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="RealMomsOfSpringfield_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="RealMomsOfSpringfield_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="timeLeft">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_IntroPanel_Timer" localise="true" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="textColumn1">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_IntroPanel_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn2">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_IntroPanel_3" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_MomsOfSpringfield_MiniEventPanel_Intro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="setVariable" boolVariable="RealMomsOfSpringfield_IntroSeen" boolValue="true" />
		<Action type="updateVariable" variable="RealMomsOfSpringfield_Store" value="true" />
		<Action type="recommendation" recType="Event" name="RealMomsOfSpringfield_OnePanel" eventName="UI_RealMomsOfSpringfield_Title" limitedTime="true" limitedTimeEndDate="RealMomsOfSpringfield_End" />
		<Action type="reparseBuildMenu" />
	</ShowEventIntro>
	<ShowEventOutro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="MomsOfSpringfieldMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="textColumn4">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_OutroPanel_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn5">
					<Attribute name="text" value="UI_RealMomsOfSpringfield_OutroPanel_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_MomsOfSpringfield_MiniEventPanel_Outro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
	</ShowEventOutro>
	<ExpiryWarning_72hr>
		<Action type="message" text="UI_RealMomsOfSpringfield_72HourExpiry_text" />
		<Action type="setVariable" boolVariable="RealMomsOfSpringfield_PreExpiryDisplayed72" boolValue="true" />
		<Action type="runScript" script="RealMomsOfSpringfield_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_72hr>
	<ExpiryWarning_24hr>
		<Action type="message" text="{FormattedExpiration24}" formatMe="root">
			<FormattingConfig path="RealMomsOfSpringfield_GameConfig:ExpirationFormattedText" />
		</Action>
		<Action type="setVariable" boolVariable="RealMomsOfSpringfield_PreExpiryDisplayed24" boolValue="true" />
		<Action type="runScript" script="RealMomsOfSpringfield_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_24hr>
	<DisplayExpiry>
		<Action type="message" text="UI_RealMomsOfSpringfield_Expired_text" />
		<Action type="clearRecommendations" category="Event" name="RealMomsOfSpringfield_OnePanel" />
		<Action type="runScript" script="RealMomsOfSpringfield_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</DisplayExpiry>
	<OpenEventHub>
		<Action type="if">
			<If expression="RealMomsOfSpringfield_EventHubTabValue == 0">
				<Then>
					<Action type="runScript" script="RealMomsOfSpringfield_Scripts:Tab_Week1" />
				</Then>
			</If>
		</Action>
	</OpenEventHub>
	<OpenMasterQuest>
		<Action type="checkRequirements">
			<Requirement type="gamestate" state="GameState_QuestProgress" />
			<True>
				<Action type="changeQuestForProgressState" name="RealMomsOfSpringfield_MasterQuest" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_QuestProgress" forceState="true">
					<Extras>
						<Attribute name="questParentDelegate" value="RealMomsOfSpringfield_MasterQuest" />
					</Extras>
				</Action>
			</False>
		</Action>
	</OpenMasterQuest>
	<ShowJobList ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="if">
					<If>
						<Requirement type="active quest" quest="TheRealMomsOfSpringfield_1" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="RealMomsOfSpringfield_Event" />
									<Attribute name="JobList" value="RealMomsOfSpringfield_JL_1" />
								</Extras>
							</Action>
						</Then>
					</If>
					<ElseIf>
						<Requirement type="active quest" quest="TheRealMomsOfSpringfield_2" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="RealMomsOfSpringfield_Event" />
									<Attribute name="JobList" value="RealMomsOfSpringfield_JL_2" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="TheRealMomsOfSpringfield_3" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="RealMomsOfSpringfield_Event" />
									<Attribute name="JobList" value="RealMomsOfSpringfield_JL_3" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="TheRealMomsOfSpringfield_4" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="RealMomsOfSpringfield_Event" />
									<Attribute name="JobList" value="RealMomsOfSpringfield_JL_4" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="TheRealMomsOfSpringfield_5" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="RealMomsOfSpringfield_Event" />
									<Attribute name="JobList" value="RealMomsOfSpringfield_JL_5" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
				</Action>
			</True>
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</ShowJobList>
	<CheckIfEnd_Week ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="quest" quest="RealMomsOfSpringfield_EventExpired" />
			<True>
				<Action type="setVariable" boolVariable="RealMomsOfSpringfield_Expired" boolValue="true" />
			</True>
		</Action>
	</CheckIfEnd_Week>
	<KitchenCarnival_Tapped parallel="true" ignoreHUD="true">
		<Action type="random">
			<Action type="anim-play" animation="Active_01_NoLoop" />
			<Action type="anim-play" animation="Active_02" />
			<Action type="anim-play" animation="Active_03" />
		</Action>
	</KitchenCarnival_Tapped>
	<AutoApply_JakeBoymanSkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="RealMomsOfSpringfield_Scripts:AutoApply_JakeBoymanSkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_JakeBoymanSkin_End>
	<AutoApply_JakeBoymanSkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="JakeBoyman" fromInventory="true" />
	</AutoApply_JakeBoymanSkin_Delayed>
	<RecommendStoreCategory ignoreHUD="true">
		<Action type="reparseBuildMenu" />
		<Action type="clearRecommendations" category="Event" name="RealMomsOfSpringfield_OnePanel" />
		<Action type="recommendation" recType="Event" name="RealMomsOfSpringfield_OnePanel" eventName="UI_RealMomsOfSpringfield_Title" limitedTime="true" limitedTimeEndDate="RealMomsOfSpringfield_End" />
		<Action type="focusOnMenuItem" menuCategory="FrontPage" />
		<Action type="reparseBuildMenu" />
	</RecommendStoreCategory>
	<EnterStoreCategory ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="RealMomsOfSpringfield_Category" />
	</EnterStoreCategory>
</ScriptPackage>