<ScriptPackage>
	<Find_Sample>
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="conditional" logic="OR">
					<Requirement type="character" name="Sample" />
					<Requirement type="characterunlocked" name="Sample" />
					<Requirement type="item" item="MirroredAndroidsDungeon" />
					<Requirement type="building" building="MirroredAndroidsDungeon" />
					<Requirement type="place building" building="MirroredAndroidsDungeon" />
				</Requirement>
			</Requirements>
			<True>
				<Action type="pantoobject" time="0" character="Sample" fakeTouch="true" />
				<Action type="focusOnMenuItem" itemName="MirroredAndroidsDungeon" menuCategory="Characters" inventory="true" />
			</True>
			<False>
				<Action type="notification" title="Character Was Not Found" button="UI_OK" 
				text="Sample Needs To Be Added To This Account" icon="sample_menu" />
				<Action type="focusOnMenuItem" itemName="MirroredAndroidsDungeon" menuCategory="Characters" />
			</False>
		</Action>
	</Find_Sample>
