<DynamicDataSelectors>
	<Selector name="Homergeddon_PTCurrencyPerFriendAction" type="table" key="FriendActionsCount">
        0,selector Homergeddon_PTCurrency_SocialUpgrade,
        30,0,
    </Selector>
	<Selector name="Homergeddon_PTCurrency_SocialUpgrade" type="table" key="Homergeddon_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__Homergeddon_GameConfig:TappablePrizeReward:FriendAmount__" />
		<TableEntry key="1" value="2 * toDouble(System::getConfigValue('__Homergeddon_GameConfig:TappablePrizeReward:FriendAmount__'))" />
	</Selector>
	<Selector name="Homergeddon_CraftingCurrencyPerFriendAction" type="table" key="FriendActionsCount">
        0,selector Homergeddon_CraftingCurrency_SocialUpgrade,
        30,0,
    </Selector>
	<Selector name="Homergeddon_CraftingCurrency_SocialUpgrade" type="table" key="Homergeddon_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__Homergeddon_GameConfig:TappableCraftReward:FriendAmount__" />
		<TableEntry key="1" value="2 * toDouble(System::getConfigValue('__Homergeddon_GameConfig:TappableCraftReward:FriendAmount__'))" />
	</Selector>
</DynamicDataSelectors>