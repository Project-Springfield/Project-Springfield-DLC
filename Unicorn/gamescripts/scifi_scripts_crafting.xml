<ScriptPackage>
	<CraftingItemLevelUp parallel="true" cache="true">
		<Action type="if">
			<If expression="lessThan(SciFi_CraftingLevel, SciFi_MaxCraftLevel)">
				<Then>
					<Action type="if">
						<If expression="greaterThanEQ(SciFi_CraftProgress, System::evaluate('Selector::SciFi_CraftingLevel_XPNeeded'))">
							<Then>
								<Action type="updateVariable" variable="SciFi_CraftProgress" value="SciFi_CraftProgress-System::evaluate('Selector::SciFi_CraftingLevel_XPNeeded')" />
								<Action type="updateVariable" variable="SciFi_CraftingLevel" value="SciFi_CraftingLevel+1" />
								<Action type="if">
									<If expression="greaterThanEQ(SciFi_CraftProgress, System::evaluate('Selector::SciFi_CraftingLevel_XPNeeded'))">
										<Then>
											<Action type="updateVariable" variable="SciFi_CraftProgress" value="0" />
										</Then>
									</If>
								</Action>
							</Then>
						</If>
					</Action>
				</Then>
			</If>
		</Action>
	</CraftingItemLevelUp>
	<HoloTulipsPurchased>
		<Action type="updateVariable" variable="HoloTulipsMax" value="__SciFi_BalancingConfig:CraftingCaps:HoloTulips__" />
		<Action type="if">
			<If expression="lessThanEQ(HoloTulips_TotalCrafted, HoloTulipsMax)">
				<Then>
					<Action type="updateVariable" variable="SF_DCS_CraftItemAssembled" value="SF_DCS_CraftItemAssembled + 1" />
					<Action type="updateVariable" variable="SF_CurrentCraftXP" value="__SciFi_BalancingConfig:Crafting:HoloTulips_1__" />
					<Action type="updateVariable" variable="SciFi_CraftProgress" value="SciFi_CraftProgress+SF_CurrentCraftXP" />
					<Action type="actionSequence" script="SciFi_Scripts_Crafting:CraftingItemLevelUp" />
				</Then>
			</If>
		</Action>
		<Action type="if">
			<If expression="HoloTulips_TotalCrafted==HoloTulipsMax">
				<Then>
					<Action type="if">
						<If expression="SF_PrototypeCompleted==0">
							<Then>
								<Action type="updateVariable" variable="SF_PrototypeCompleted" value="1" />
							</Then>
						</If>
					</Action>
					<Action type="notification" text="UI_SciFi_Crafting_DemoComplete" title="BLD_HoloTulips_name" revealSpeed="medium" icon="HoloTulips_Menu" button="UI_OK" />
				</Then>
			</If>
			<ElseIf expression="greaterThan(HoloTulips_TotalCrafted, HoloTulipsMax)">
				<Then>
					<Action type="updateVariable" variable="SF_DCS_CraftItemPurchased" value="SF_DCS_CraftItemPurchased + 1" />
					<Action type="if">
						<If expression="SF_CraftItemPurchased==0">
							<Then>
								<Action type="updateVariable" variable="SF_CraftItemPurchased" value="1" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
		</Action>
	</HoloTulipsPurchased>
