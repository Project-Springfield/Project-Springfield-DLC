					</Action>
				</Then>
			</Else>
		</Action>
	</MasterTappable_Gained_Friend>
	<SpawnMasterTappableOverTime ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="relative time" defaultTime="_[THOH2015_CampfireSpawnTimeStart]_" startOffset="5s" />
			</Requirements>
			<True>
				<Action type="setVariable" intVariable="TappablesToSpawn" intValue="__THOH2015_GameConfig:Tappables:SpawnOverTime_NumSpawn__" />
				<Action type="createobject" name="THOH2015_MasterTappable" count="TappablesToSpawn" save="true" panCamera="false" building="SpookyCampfire" awayFromSpawnLoc="200.0f" prefAwayGridType="road|beach|grass|pavement|boardwalk|boardwalkedge" />
			</True>
			<False>
				<Action type="setVariable" intVariable="TappablesToSpawn" intValue="__THOH2015_GameConfig:Tappables:SpawnOverTime_NumSpawn__" />
				<Action type="createobject" name="THOH2015_MasterTappable" count="TappablesToSpawn" save="true" panCamera="false" />
			</False>
		</Action>
	</SpawnMasterTappableOverTime>
