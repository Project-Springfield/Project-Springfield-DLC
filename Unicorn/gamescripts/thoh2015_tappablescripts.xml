<ScriptPackage>
	<MasterTappable_Gained parallel="true" cache="true">
		<Action type="if" precheck="true">
			<If>
				<Requirements>
					<Requirement type="formula" formula="greaterThan(THOH2015_SpawnCounter_Snakes, 0)" />
					<Requirement type="variable" variable="boolUseSnakeDeco" />
				</Requirements>
				<Then>
					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_Snakes" intValue="THOH2015_SpawnCounter_Snakes-1" />
					<Action type="skin-set" skin="THOH2015_Snake" />
					<Action type="skin-set" skin="THOH2015_Snake2" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Snake" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</If>
			<ElseIf>
				<Requirements>
					<Requirement type="formula" formula="greaterThan(THOH2015_SpawnCounter_Zombies, 0)" />
					<Requirement type="variable" variable="boolUseZombieDeco" />
				</Requirements>
				<Then>
					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_Zombies" intValue="THOH2015_SpawnCounter_Zombies-1" />
					<Action type="setObjectVariable" variable="CurrentSkin" value="1" />
					<Action type="skin-set" skin="THOH2015_Zombie" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Zombie" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirements>
					<Requirement type="formula" formula="greaterThan(THOH2015_SpawnCounter_Rabbits, 0)" />
					<Requirement type="variable" variable="boolUseRabbitDeco" />
				</Requirements>
				<Then>
					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_Rabbits" intValue="THOH2015_SpawnCounter_Rabbits-1" />
					<Action type="setObjectVariable" variable="CurrentSkin" value="2" />
					<Action type="random">
						<Action type="skin-set" skin="THOH2015_Rabbit2" />
						<Action type="skin-set" skin="THOH2015_Rabbit" />
					</Action>
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Rabbit" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
