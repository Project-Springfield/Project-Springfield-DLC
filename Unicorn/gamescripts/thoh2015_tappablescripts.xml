						<Requirement type="variable" variable="THOH2015_CurrentTappable" count="1" />
					</Requirement>
					<Requirement type="friendvariable" variable="THOH2015_CurrentTappable" count="1" />
				</Requirement>
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="1" />
					<Action type="skin-set" skin="THOH2015_Zombie" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Zombie" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<Else>
