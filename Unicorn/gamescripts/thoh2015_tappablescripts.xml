					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_Rabbits" intValue="THOH2015_SpawnCounter_Rabbits-1" />
					<Action type="setObjectVariable" variable="CurrentSkin" value="2" />
					<Action type="random">
						<Action type="skin-set" skin="THOH2015_Rabbit2" />
						<Action type="skin-set" skin="THOH2015_Rabbit" />
					</Action>
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Rabbit" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="formula" formula="greaterThan(THOH2015_SpawnCounter_Skeletons, 0)" />
				<Then>
					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_Skeletons" intValue="THOH2015_SpawnCounter_Skeletons-1" />
					<Action type="setObjectVariable" variable="CurrentSkin" value="3" />
					<Action type="skin-set" skin="THOH2015_Skeleton" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Skeleton" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="formula" formula="greaterThan(THOH2015_SpawnCounter_StarSpawn, 0)" />
				<Then>
					<Action type="setVariable" intVariable="THOH2015_SpawnCounter_StarSpawn" intValue="THOH2015_SpawnCounter_StarSpawn-1" />
					<Action type="setObjectVariable" variable="CurrentSkin" value="4" />
					<Action type="skin-set" skin="THOH2015_StarSpawn" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_StarSpawn" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="variable" variable="THOH2015_CurrentTappable" count="4" />
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="4" />
					<Action type="skin-set" skin="THOH2015_StarSpawn" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_StarSpawn" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="variable" variable="THOH2015_CurrentTappable" count="3" />
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="3" />
					<Action type="skin-set" skin="THOH2015_Skeleton" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Skeleton" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="variable" variable="THOH2015_CurrentTappable" count="2" />
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="2" />
					<Action type="random">
						<Action type="skin-set" skin="THOH2015_Rabbit2" />
						<Action type="skin-set" skin="THOH2015_Rabbit" />
					</Action>
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Rabbit" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="variable" variable="THOH2015_CurrentTappable" count="1" />
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="1" />
					<Action type="skin-set" skin="THOH2015_Zombie" />
					<Action type="if">
						<If expression="BankedMasterTappables==0">
							<Then>
								<Action type="runScript" script="SpawnSound_Zombie" package="THOH2015_TappableScripts" />
							</Then>
						</If>
					</Action>
				</Then>
			</ElseIf>
			<Else>
				<Then>
					<Action type="setObjectVariable" variable="CurrentSkin" value="0" />
					<Action type="random">
						<Action type="skin-set" skin="THOH2015_Snake" />
						<Action type="skin-set" skin="THOH2015_Snake2" />
						<Action type="if">
							<If expression="BankedMasterTappables==0">
								<Then>
									<Action type="runScript" script="SpawnSound_Snake" package="THOH2015_TappableScripts" />
								</Then>
							</If>
						</Action>
					</Action>
				</Then>
			</Else>
		</Action>
	</MasterTappable_Gained>
