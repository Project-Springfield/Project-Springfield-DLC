<Attributes>
	<MedievalAttack>
		<Attribute name="buildingUnderAttackAnimation" value="Attack" />
		<Attribute name="buildingUnderAttackIcon" value="Indicator_Defend.rgb" />
		<Attribute name="buildingDestroyedIcon" value="Indicator_Repair.rgb" />
		<Attribute name="offlineVariableNumber" value="SPECIAL_EVENT_OFFLINE_NUMBER" />
		<Attribute name="friendAttacksTimeGate" value="Medieval:FriendAttacks" />
		<Attribute name="friendAttacksAvailable" value="UI_firstFriendAttacksAvailable_msg" />
		<Attribute name="friendAttacksUnavailable" value="UI_friendAttacksUnavailable_msg" />
		<Attribute name="randomAttacksTimeGate" value="Medieval:RandomAttacks" />
		<Attribute name="randomAttacksAvailable" value="UI_randomAttacksAvailable_msg" />
		<Attribute name="offlineCurrencies">
			<AttributeSet>
				<Attribute name="offlineCurrencyReward" value="Elixir" />
				<Attribute name="offlineVariableAmount" value="SPECIAL_EVENT_OFFLINE_AMOUNT_1" />
				<Attribute name="offlineVariableType" value="SPECIAL_EVENT_OFFLINE_TYPE_1" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="offlineCurrencyReward" value="Gold" />
				<Attribute name="offlineVariableAmount" value="SPECIAL_EVENT_OFFLINE_AMOUNT_2" />
				<Attribute name="offlineVariableType" value="SPECIAL_EVENT_OFFLINE_TYPE_2" />
			</AttributeSet>
		</Attribute>
		<Attribute name="defaultFriendName" value="UI_defaultFriendName_msg" />
		<Attribute name="attackingRewardMessages">
			<AttributeSet>
				<Attribute name="message" value="UI_DefendTap01_msg" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="message" value="UI_DefendTap02_msg" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="message" value="UI_DefendTap03_msg" />
			</AttributeSet>
		</Attribute>
		<Attribute name="wreckedRewardMessages">
			<AttributeSet>
				<Attribute name="message" value="UI_WreckTap01_msg" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="message" value="UI_WreckTap02_msg" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="message" value="UI_WreckTap03_msg" />
			</AttributeSet>
		</Attribute>
		<Attribute name="fakeFriendCooldown" value="4h" />
		<Attribute name="onDestroyedBuildingFromMenu" value="MedievalTimes_Scripts:PlayDestroyOneShot" />
		<Attribute name="doScriptsWithFirstAttack">
			<AttributeSet>
				<Attribute name="ignoreOnZeroLifeSpan" value="true" />
				<Attribute name="scriptUsesVarInDataSelector" value="Object::NumTroopsInAttackLocal" />
				<Attribute name="myScript" value="MedievalTimes_Scripts:SpawnBlueInitialAttackTroopInBattle" />
				<Attribute name="theirScript" value="MedievalTimes_Scripts:SpawnRedInitialAttackTroopInBattle" />
			</AttributeSet>
		</Attribute>
		<Attribute name="doScriptsWithAttack">
			<AttributeSet>
				<Attribute name="ignoreOnZeroLifeSpan" value="true" />
				<Attribute name="scriptUsesVarInDataSelector" value="Object::NumTroopsInAttackLocal" />
				<Attribute name="myScript" value="MedievalTimes_Scripts:SpawnBlueAttackTroopInBattle" />
				<Attribute name="theirScript" value="MedievalTimes_Scripts:SpawnRedAttackTroopInBattle" />
			</AttributeSet>
		</Attribute>
		<Attribute name="doScriptsWithAttackRandomly">
			<AttributeSet>
				<Attribute name="ignoreOnZeroLifeSpan" value="true" />
				<Attribute name="scriptUsesVarInDataSelector" value="Object::NumTroopsInAttackLocal" />
				<Attribute name="theirScript" value="MedievalTimes_Scripts:SpawnRedAttackTroopInBattleRandomly" />
			</AttributeSet>
		</Attribute>
		<Attribute name="doScriptsWhenAttackRemoved">
			<AttributeSet>
				<Attribute name="scriptUsesVarInDataSelector" value="Object::NumTroopsInAttackLocal" />
				<Attribute name="checkGroupChars" value="NPCTroops" />
				<Attribute name="theirScript" value="MedievalTimes_Scripts:RemoveRedAttackTroopFromBattle" />
			</AttributeSet>
		</Attribute>
		<OnOfflineScript>
			<Script name="OffLineReward" package="MedievalTimes_Scripts" />
		</OnOfflineScript>
		<Attribute name="attacksMaySpillOutRewards" value="true" />
		<Attribute name="spillOutRewardReqs">
			<AttributeSet>
				<Attribute name="attackReqVar" value="lifespan" />
				<Attribute name="attackReqVarFormula" value="lessThan(attackReqVar,1)" />
			</AttributeSet>
		</Attribute>
		<Attribute name="Counters">
			<AttributeSet>
				<Attribute name="TotalPending">
					<AttributeSet>
						<Attribute name="variable" value="CurBeingAttacked" />
					</AttributeSet>
				</Attribute>
				<Attribute name="TotalDestroyed">
					<AttributeSet>
						<Attribute name="variable" value="CurBuildingsDestroyed" />
					</AttributeSet>
				</Attribute>
				<Attribute name="TotalAttacks">
					<AttributeSet>
						<Attribute name="variable" value="CurTotalAttacked" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
		</Attribute>
		<OnTimeOut group="Castles">
			<FriendAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendCastleAttackerWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendCastleAttackerWon:Gold__" money="__MedievalTimes_GameConfig:VSfriendCastleAttackerWon:Money__" xp="__MedievalTimes_GameConfig:VSfriendCastleAttackerWon:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendCastleDefenderLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendCastleDefenderLost:Gold__" money="__MedievalTimes_GameConfig:VSfriendCastleDefenderLost:Money__" xp="__MedievalTimes_GameConfig:VSfriendCastleDefenderLost:XP__" />
				</LandOwnerReward>
			</FriendAttack>
			<RandomAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomCastleAttackerWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomCastleAttackerWon:Gold__" money="__MedievalTimes_GameConfig:VSrandomCastleAttackerWon:Money__" xp="__MedievalTimes_GameConfig:VSrandomCastleAttackerWon:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomCastleDefenderLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomCastleDefenderLost:Gold__" money="__MedievalTimes_GameConfig:VSrandomCastleDefenderLost:Money__" xp="__MedievalTimes_GameConfig:VSrandomCastleDefenderLost:XP__" />
				</LandOwnerReward>
			</RandomAttack>
		</OnTimeOut>
		<OnCollected group="Castles">
			<FriendAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendCastleAttackerLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendCastleAttackerLost:Gold__" money="__MedievalTimes_GameConfig:VSfriendCastleAttackerLost:Money__" xp="__MedievalTimes_GameConfig:VSfriendCastleAttackerLost:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendCastleDefenderWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendCastleDefenderWon:Gold__" money="__MedievalTimes_GameConfig:VSfriendCastleDefenderWon:Money__" xp="__MedievalTimes_GameConfig:VSfriendCastleDefenderWon:XP__" />
				</LandOwnerReward>
			</FriendAttack>
			<RandomAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomCastleAttackerLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomCastleAttackerLost:Gold__" money="__MedievalTimes_GameConfig:VSrandomCastleAttackerLost:Money__" xp="__MedievalTimes_GameConfig:VSrandomCastleAttackerLost:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomCastleDefenderWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomCastleDefenderWon:Gold__" money="__MedievalTimes_GameConfig:VSrandomCastleDefenderWon:Money__" xp="__MedievalTimes_GameConfig:VSrandomCastleDefenderWon:XP__" />
				</LandOwnerReward>
			</RandomAttack>
		</OnCollected>
		<OnTimeOut group="Default">
			<FriendAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendHouseAttackerWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendHouseAttackerWon:Gold__" money="__MedievalTimes_GameConfig:VSfriendHouseAttackerWon:Money__" xp="__MedievalTimes_GameConfig:VSfriendHouseAttackerWon:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendHouseDefenderLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendHouseDefenderLost:Gold__" money="__MedievalTimes_GameConfig:VSfriendHouseDefenderLost:Money__" xp="__MedievalTimes_GameConfig:VSfriendHouseDefenderLost:XP__" />
				</LandOwnerReward>
			</FriendAttack>
			<RandomAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomHouseAttackerWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomHouseAttackerWon:Gold__" money="__MedievalTimes_GameConfig:VSrandomHouseAttackerWon:Money__" xp="__MedievalTimes_GameConfig:VSrandomHouseAttackerWon:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomHouseDefenderLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomHouseDefenderLost:Gold__" money="__MedievalTimes_GameConfig:VSrandomHouseDefenderLost:Money__" xp="__MedievalTimes_GameConfig:VSrandomHouseDefenderLost:XP__" />
				</LandOwnerReward>
			</RandomAttack>
		</OnTimeOut>
		<OnCollected group="Default">
			<FriendAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendHouseAttackerLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendHouseAttackerLost:Gold__" money="__MedievalTimes_GameConfig:VSfriendHouseAttackerLost:Money__" xp="__MedievalTimes_GameConfig:VSfriendHouseAttackerLost:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSfriendHouseDefenderWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSfriendHouseDefenderWon:Gold__" money="__MedievalTimes_GameConfig:VSfriendHouseDefenderWon:Money__" xp="__MedievalTimes_GameConfig:VSfriendHouseDefenderWon:XP__" />
				</LandOwnerReward>
			</FriendAttack>
			<RandomAttack>
				<CreatorReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomHouseAttackerLost:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomHouseAttackerLost:Gold__" money="__MedievalTimes_GameConfig:VSrandomHouseAttackerLost:Money__" xp="__MedievalTimes_GameConfig:VSrandomHouseAttackerLost:XP__" />
				</CreatorReward>
				<LandOwnerReward>
					<Reward Elixir="__MedievalTimes_GameConfig:VSrandomHouseDefenderWon:Elixir__" Gold="__MedievalTimes_GameConfig:VSrandomHouseDefenderWon:Gold__" money="__MedievalTimes_GameConfig:VSrandomHouseDefenderWon:Money__" xp="__MedievalTimes_GameConfig:VSrandomHouseDefenderWon:XP__" />
				</LandOwnerReward>
			</RandomAttack>
		</OnCollected>
		<AttackPeerRequirements>
			<Requirement type="conditional" logic="OR">
				<Requirement type="friendvariable" variable="Medieval" />
				<Requirement type="fakefriend" />
			</Requirement>
			<Requirement type="conditional" logic="OR">
				<Requirement type="conditional" logic="AND">
					<Requirement type="friend land" not="true" />
					<Requirement type="timegate" timegate="Medieval:RandomAttacks" instanceIdSource="peerId" />
				</Requirement>
				<Requirement type="conditional" logic="AND">
					<Requirement type="friend land" />
					<Requirement type="timegate" timegate="Medieval:FriendAttacks" instanceIdSource="friendId" />
				</Requirement>
			</Requirement>
		</AttackPeerRequirements>
		<DailyAttacksUIRequirements>
			<Requirement type="conditional" logic="OR">
				<Requirement type="friendvariable" variable="Medieval" />
				<Requirement type="fakefriend" />
			</Requirement>
		</DailyAttacksUIRequirements>
	</MedievalAttack>
</Attributes>