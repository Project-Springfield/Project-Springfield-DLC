				<Then>
					<Action type="reward" save="true" Casino_ActivePrizeTrackCurrency="selector CasinoPT2CurrencyPerFriendAction" suppress_sound="true" />
				</Then>
			</ElseIf>
			<ElseIf>
				<Requirement type="reqList" location="Casino_GameConfig:Requirement_Act3" />
				<Then>
					<Action type="reward" save="true" Casino_ActivePrizeTrackCurrency="selector CasinoPT3CurrencyPerFriendAction" suppress_sound="true" />
