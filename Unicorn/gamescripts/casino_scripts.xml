		<Action type="sound" sound="Gambler_Tap" />
		<Action type="stop" />
		<Action type="if" precheck="true">
			<If expression="[Object::SkinID]==0" asObjectExpression="true">
				<Then>
					<Action type="anim-remap" character="Gambler">
						<AnimationRemap Front_Walk="Front_Walk_fast" Back_Walk="Back_Walk_fast" />
					</Action>
				</Then>
			</If>
			<ElseIf expression="[Object::SkinID]==138006" asObjectExpression="true">
				<Then>
					<Action type="anim-remap" character="Gambler">
						<AnimationRemap Front_Walk="Gambler02_Front_Walk_fast" Back_Walk="Gambler02_Back_Walk_fast" />
