						<AnimationRemap Front_Walk="Gambler03_Front_Walk_fast" Back_Walk="Gambler03_Back_Walk_fast" />
					</Action>
				</Then>
			</ElseIf>
			<ElseIf expression="[Object::SkinID]==138008" asObjectExpression="true">
				<Then>
					<Action type="anim-remap" character="Gambler">
						<AnimationRemap Front_Walk="Gambler04_Front_Walk_fast" Back_Walk="Gambler04_Back_Walk_fast" />
					</Action>
				</Then>
			</ElseIf>
		</Action>
