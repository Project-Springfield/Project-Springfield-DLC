<Attributes>
	<ElfCannonMenu></ElfCannonMenu>
	<ElfCannonControlMenu>
		<Attribute name="customStatusTitle" value="UI_CannonControlMenu_title" localise="true" parameters="messageParams">
			<AttributeSet name="messageParams">
				<Attribute name="0" formula="System::evaluate('Building::ElfCannonControl::Object::ElfCannonLevel')+1" />
			</AttributeSet>
		</Attribute>
		<Attribute name="menus">
			<Requirement type="variable" variable="XMAS2014_Pt2" />
			<AttributeSet name="3LineMessage">
				<Attribute name="config">
					<AttributeSet name="line1">
						<Attribute name="text" value="UI_CannonStats_ElvesPerShot" localise="true" parameters="messageParams">
							<AttributeSet name="messageParams">
								<Attribute name="0" formula="System::evaluate('Selector::CanonFireAmount')" />
							</AttributeSet>
						</Attribute>
						<Attribute name="font" value="eFont_Small" />
						<Attribute name="color" value="FFFFFFFF" />
					</AttributeSet>
					<AttributeSet name="line2">
						<Attribute name="text" value="UI_CannonStats_BellsPerElf" localise="true" parameters="messageParams">
							<AttributeSet name="messageParams">
								<Attribute name="0" formula="System::evaluate('Selector::CanonBerriesPerLevel')" />
							</AttributeSet>
						</Attribute>
						<Attribute name="font" value="eFont_Small" />
						<Attribute name="color" value="FFFFFFFF" />
					</AttributeSet>
					<AttributeSet name="line3">
						<Attribute name="text" value="UI_CannonStats_Cooldown" localise="true" parameters="messageParams">
							<AttributeSet name="messageParams">
								<Attribute name="0" formula="System::evaluate('Selector::CanonHoursToFirePerLevel')" />
								<Attribute name="1" formula="System::evaluate('Selector::CanonMinutesToFirePerLevel')" />
							</AttributeSet>
						</Attribute>
						<Attribute name="font" value="eFont_Small" />
						<Attribute name="color" value="FFFFFFFF" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
		</Attribute>
	</ElfCannonControlMenu>
</Attributes>