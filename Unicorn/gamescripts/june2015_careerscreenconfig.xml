<Attributes>
	<MenuConfig>
		<Attribute name="configRoot">
			<AttributeSet name="titleText">
				<Attribute name="text" value="UI_June2015_CareerStats_title" localise="true" />
				<Attribute name="font" value="eFont_Special" />
				<Attribute name="color" value="FFFFFFFF" />
			</AttributeSet>
			<AttributeSet name="overviewLabel1">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_total" localise="true" />
			</AttributeSet>
			<AttributeSet name="overviewLabel2">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_wins" localise="true" parameters="textParams">
					<AttributeSet name="textParams">
						<Attribute name="0" formula="passiveGamesWon + activeGamesWon" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="overviewLabel3">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_losses" localise="true" parameters="textParams">
					<AttributeSet name="textParams">
						<Attribute name="0" formula="passiveGamesLost + activeGamesLost" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="overviewLabel4">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_draws" localise="true" parameters="textParams">
					<AttributeSet name="textParams">
						<Attribute name="0" formula="passiveGamesTied + activeGamesTied" />
					</AttributeSet>
				</Attribute>
			</AttributeSet>
			<AttributeSet name="ratedEntryText">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_opponent" localise="true" />
			</AttributeSet>
			<AttributeSet name="nonRatedEntryText">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_opponent" localise="true" />
			</AttributeSet>
			<AttributeSet name="goButtonLabel">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="FFFFFFFF" />
				<Attribute name="text" value="UI_June2015_CareerStats_rematch" localise="true" />
			</AttributeSet>
			<AttributeSet name="goButtonLabelHL">
				<Attribute name="font" value="eFont_NormalBold" />
				<Attribute name="color" value="000000FF" />
				<Attribute name="text" value="UI_June2015_CareerStats_rematch" localise="true" />
			</AttributeSet>
			<AttributeSet name="goButton">
				<Attribute name="color" value="0091F1FF" />
			</AttributeSet>
		</Attribute>
	</MenuConfig>
	<DataConfig>
		<Attribute name="entries">
			<AttributeSet>
				<Attribute name="specialEvent" value="Homerball" />
				<Attribute name="buffer" value="PassiveMatches" />
				<Attribute name="entryTextFormat_self" value="UI_June2015_AnonName" localise="true" />
				<Attribute name="entryTextFormat_friend" value="{Object::Username}" />
				<Attribute name="entryTextFormat_opponent" value="UI_June2015_AnonName" localise="true" />
				<Attribute name="summaryTextFormat_SelectorFormula" value="ParamSet::FromIntKey::RemoteUserScore - ParamSet::FromIntKey::LocalUserScore" />
				<Attribute name="summaryTextFormat_SelectorTable">
					<AttributeSet>
						<Attribute name="value" value="UI_June2015_CareerStats_winscore" localise="true" parameters="textParams">
							<AttributeSet name="textParams">
								<Attribute name="0" value="{ParamSet::FromIntKey::LocalUserScore}" />
								<Attribute name="1" value="{ParamSet::FromIntKey::RemoteUserScore}" />
							</AttributeSet>
						</Attribute>
					</AttributeSet>
					<AttributeSet>
						<Attribute name="threshold" value="0" />
						<Attribute name="value" value="UI_June2015_CareerStats_tiescore" localise="true" parameters="textParams">
							<AttributeSet name="textParams">
								<Attribute name="0" value="{ParamSet::FromIntKey::LocalUserScore}" />
								<Attribute name="1" value="{ParamSet::FromIntKey::LocalUserScore}" />
							</AttributeSet>
						</Attribute>
					</AttributeSet>
					<AttributeSet>
						<Attribute name="threshold" value="1" />
						<Attribute name="value" value="UI_June2015_CareerStats_losescore" localise="true" parameters="textParams">
							<AttributeSet name="textParams">
								<Attribute name="0" value="{ParamSet::FromIntKey::RemoteUserScore}" />
								<Attribute name="1" value="{ParamSet::FromIntKey::LocalUserScore}" />
							</AttributeSet>
						</Attribute>
					</AttributeSet>
				</Attribute>
				<Attribute name="buttonTextFormat_SelectorFormula" value="ParamSet::FromIntKey::RemoteUserScore &gt; ParamSet::FromIntKey::LocalUserScore" />
				<Attribute name="buttonTextFormat_SelectorTable">
					<AttributeSet>
						<Attribute name="value" value="UI_June2015_CareerStats_rematch" localise="true" />
					</AttributeSet>
					<AttributeSet>
						<Attribute name="threshold" value="1" />
						<Attribute name="value" value="UI_June2015_CareerStats_revenge" localise="true" />
					</AttributeSet>
				</Attribute>
				<Attribute name="buttonScript" value="June2015_Scripts:RevengeMatch" />
				<Attribute name="buttonScript_unvisitable" value="June2015_Scripts:ChallengeUnvisitableFriend" />
				<Attribute name="matchmakingCategoryForRevenge" value="Homerball2015" />
				<Attribute name="ratingFormula" value="ParamSet::FromIntKey::RemoteUserRating" />
				<Attribute name="ratingConfigPath" value="June2015_HomerballConfig:LargeStarRating" />
			</AttributeSet>
		</Attribute>
	</DataConfig>
</Attributes>