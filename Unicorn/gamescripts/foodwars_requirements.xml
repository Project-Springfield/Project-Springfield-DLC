<Requirements>
	<StoreCategory_FoodWars_Unlocked>
		<Requirement type="quest" quest="MyPad" />
		<Requirement type="time" start="FoodWars_Start" end="FoodWars_End" />
		<Requirement type="quest started" quest="FoodWars_IntroPanel" />
	</StoreCategory_FoodWars_Unlocked>
	<ShatteredDonuts_Available>
		<Requirement type="conditional" logic="OR">
			<Requirement type="quest" quest="VictoryWithASideOfFries" />
			<Requirement type="time" start="FoodWars_End" end="FoodWars_ShatteredDreams_End" triggerEvent="true" />
		</Requirement>
		<Requirement type="time" end="FoodWars_ShatteredDreams_End" triggerEvent="true" />
	</ShatteredDonuts_Available>
	<MasterQuestActive>
		<Requirement type="conditional" logic="OR">
			<Requirement type="active quest" quest="ClimbingHamburgerHill_1" />
			<Requirement type="active quest" quest="ClimbingHamburgerHill_2" />
			<Requirement type="active quest" quest="ClimbingHamburgerHill_3" />
			<Requirement type="active quest" quest="ClimbingHamburgerHill_4" />
			<Requirement type="active quest" quest="ClimbingHamburgerHill_5" />
		</Requirement>
	</MasterQuestActive>
	<FoodWars_TrayOfDonuts_Visible>
		<Requirement type="reqList" location="FoodWars_Requirements:MTXPurchaseOfferVisibility" />
		<Requirement type="time" start="FoodWars_Start" end="FoodWars_End" />
		<Requirement type="reqList" location="FoodWars_Requirements:StoreCategory_FoodWars_Unlocked" />
		<Requirement type="variable" variable="FoodWars_TrayOfDonuts_Purchased" not="true" />
	</FoodWars_TrayOfDonuts_Visible>
	<MTXPurchaseOfferVisibility>
		<Requirements>
			<Requirement type="conditional" logic="OR">
				<Requirement type="conditional" logic="AND">
					<Requirement type="platform" platform="ios" />
					<Requirement type="config" key="FoodWars_GameConfig:EnableMTX:FoodWars_iOSMTXOffersEnabled" value="true" />
				</Requirement>
				<Requirement type="conditional" logic="AND">
					<Requirement type="platform" platform="android" />
					<Requirement type="config" key="FoodWars_GameConfig:EnableMTX:FoodWars_AndroidMTXOffersEnabled" value="true" />
				</Requirement>
			</Requirement>
		</Requirements>
	</MTXPurchaseOfferVisibility>
</Requirements>