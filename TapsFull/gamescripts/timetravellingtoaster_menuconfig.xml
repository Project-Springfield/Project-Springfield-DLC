<Attributes>
	<StoreShortcut>
		<Attribute>
			<Attribute name="StoreRule" value="XMAS2014ShortCutTriple" />
			<Attribute name="StoreIcon" value="BSE_TToaster_ThreePanel" />
		</Attribute>
		<Attribute>
			<Requirements>
				<Requirement type="conditional" logic="OR">
					<Requirement type="time" start="TTT_ThreePanelEnd" />
					<Requirement type="time" start="DonutDay2017_Start" end="DonutDay2017_End" />
					<Requirement type="conditional" logic="AND">
						<Requirement type="time" start="TTT_Store_ConcertHall_Start" end="TTT_Store_ConcertHall_End" />
						<Requirement type="building owned" building="ConcertHall" not="true" />
					</Requirement>
				</Requirement>
			</Requirements>
			<Attribute name="StoreRule" value="XMAS2014ShortCutSingle" />
			<Attribute name="StoreIcon" value="ICO_STOR_Single_TimeTravellingToaster" />
		</Attribute>
	</StoreShortcut>
