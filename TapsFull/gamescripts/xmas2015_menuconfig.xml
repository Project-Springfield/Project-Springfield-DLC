	<HotSquisheeStationLevelUpMenu>
		<Attribute name="enabled" value="true" />
		<Attribute>
			<Requirement type="players land" not="true" />
			<Attribute name="enabled" value="false" />
		</Attribute>
		<Attribute name="maxLevel" value="10000" />
		<Attribute name="currentLevel" formula="Object::BuildingLevel+1" />
		<Attribute name="nextLevel" formula="Object::BuildingLevel+2" />
		<Attribute name="useObjectVariable" value="Object::BuildingLevel" />
		<Attribute name="closeMenuAfterUpgrade" value="false" />
		<Attribute name="soundOnLevelUpButtonPress" value="sfx_Winter2015_Hot_Squishee_Station_Upgrade.caf" />
		<Attribute name="levelTextLine1" value="UI_BuildingLevel" localise="true" parameters="levelTextLine1">
			<AttributeSet name="levelTextLine1">
				<Attribute name="0" pointer="currentLevel" />
			</AttributeSet>
		</Attribute>
		<Attribute name="levelTextLine2" value="UI_XMAS2015_HotSquisheeStation_nextLevel" localise="true" parameters="levelTextLine2">
			<AttributeSet name="levelTextLine2">
				<Attribute name="0" pointer="nextLevel" />
			</AttributeSet>
		</Attribute>
		<Attribute name="maxLevelText" value="UI_maxLevelText_name" />
		<Attribute name="turnOnGetMoreUI" value="false" />
		<Attribute name="turnOnTopUpUI" value="true" />
		<Attribute name="topUpConfigNextLevel" formula="Object::BuildingLevel+2" />
		<Attribute name="topUpConfigTitleText" value="UI_UpgradeButton_name" localise="true" />
		<Attribute name="topUpConfigBodyText" value="UI_GeneralUpgrade" localise="true" />
		<Attribute name="topUpConfigBodyImage" value="ICO_XMAS2015_HotSquisheeStation_Popup" />
		<Attribute name="notEnoughTitleText" value="UI_XMAS2015_HotSquisheeStation_notEnoughCurrency_title" />
		<Attribute name="levelUpCurrencyBarType" value="CurrencyBar" />
		<Attribute name="headerCurrencies">
			<AttributeSet>
				<Attribute name="currency" value="LoyaltyStamps" />
			</AttributeSet>
			<AttributeSet>
				<Attribute name="currency" value="donuts" />
			</AttributeSet>
		</Attribute>
		<Attribute name="GetMoreText" value="GEN_GetMore" />
		<Attribute name="GetMoreTextFontSize" value="eFont_XSBold" />
		<Attribute name="CurrencyBarFontSize" value="eFont_SmallBold" />
		<Attribute name="levelUpIcon" value="ICO_XMAS2015_SquisheeStationUpgrade_MD.rgb" />
		<Attribute name="NextLevelText" value="UI_XMAS2015_HotSquisheeStation_nextLevel_subtext1" localise="true" />
		<Attribute name="NextLevelText2" value="UI_XMAS2015_HotSquisheeStation_nextLevel_subtext2" localise="true" />
		<Attribute name="getMoreTitle" value="BLD_HotSquisheeStation_name" localise="true" />
		<Attribute>
			<Cost LoyaltyStamps="__XMAS2015_GameConfig:HotSquisheeStation:LevelCostFormula__" />
			<Attribute>
				<Attribute name="levelTextLine3" pointer="NextLevelText" localise="true" parameters="levelTextData">
					<AttributeSet name="levelTextData">
						<Attribute name="0" value="0" />
					</AttributeSet>
				</Attribute>
				<Attribute name="levelTextLine4" pointer="NextLevelText2" localise="true" parameters="levelTextData">
					<AttributeSet name="levelTextData">
						<Attribute name="0" value="0" />
					</AttributeSet>
				</Attribute>
				<Attribute name="levelUpImage" pointer="levelUpIcon" />
				<Attribute name="onLevelUpgradeScript" value="XMAS2015_Scripts:UpgradeHotSquisheeStation" />
				<Attribute name="levelTextLine4Image" pointer="ratingImage" />
			</Attribute>
		</Attribute>
	</HotSquisheeStationLevelUpMenu>
	<HotSquisheeStationLevelUpMenu_NonEvent>
		<Attribute name="enabled" value="true" />
		<Attribute>
			<Requirement type="players land" not="true" />
			<Attribute name="enabled" value="false" />
		</Attribute>
		<Attribute>
			<Requirement type="conditional" logic="OR">
				<Requirement type="players land" not="true" />
				<Requirement type="formula" formula="Object::BuildingLevel &gt; 0" not="true" />
			</Requirement>
			<Attribute name="enabled" value="false" />
		</Attribute>
		<Attribute name="displayOnly" value="true" />
		<Attribute name="currentLevel" formula="Object::BuildingLevel+1" />
		<Attribute name="useObjectVariable" value="Object::BuildingLevel+1" />
		<Attribute name="levelTextLine1" value="UI_BuildingLevel" localise="true" parameters="levelTextLine1">
			<AttributeSet name="levelTextLine1">
				<Attribute name="0" pointer="currentLevel" />
			</AttributeSet>
		</Attribute>
	</HotSquisheeStationLevelUpMenu_NonEvent>
