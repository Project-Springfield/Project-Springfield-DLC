		<Attribute name="ItemCraftedDoneTitleText" value="BLD_ClawMachine_name" localise="true" />
		<Attribute name="ItemCraftedStoreButtonColor" value="0x13711EFF" />
		<Attribute name="ItemCraftedDoneMessageText" program="program">
			<Program><![CDATA[
                #program;
                
                var startStringKey = 'UI_ClawMachine_Common';
                if(System::evaluate('Building::ClawMachine::Object::IsClawResultRare'))
                {
                    startStringKey = 'UI_ClawMachine_Rare';
                }
                
                var tempString = System::localise(startStringKey);
                tempString += Building::getTextpoolName(Building::find(System::evaluate('Building::ClawMachine::Object::ClawResult')));
                tempString += '\n';
                tempString += System::localise('UI_Crafting_Storage');
                return tempString;
            ]]></Program>
		</Attribute>
		<Attribute name="ItemCraftedNonplaceableDoneMessageText" program="program" localise="true">
			<Program><![CDATA[
                #program;
                
                var startStringKey = 'UI_ClawMachine_Common';
                if(System::evaluate('Building::ClawMachine::Object::IsClawResultRare'))
                {
                startStringKey = 'UI_ClawMachine_Rare';
                }
                
                var tempString = System::localise(startStringKey);
                tempString += Building::getTextpoolName(Building::find(System::evaluate('Building::ClawMachine::Object::ClawResult')));
                tempString += '\n';
                tempString += System::localise('UI_Crafting_Nonplaceable_Crafted');
                return tempString;
            ]]></Program>
		</Attribute>
		<Attribute name="ItemCraftedOKText" value="GEN_OK" localise="true" />
	</ClawMachine_RewardWithPlaceStore>
	<ClawMachine_FormattedText>
		<Attribute name="CommonShoesDuringEventText" program="program">
			<Program><![CDATA[
                #program;
                
                var tempString = System::localise('UI_ClawMachine_Common');
                tempString += toString(System::evaluate('Building::ClawMachine::Object::ClawResult'));
                tempString += '\n';
                tempString += System::localise('UI_PinPals_BowlingShoesSingular');
                return tempString;
            ]]></Program>
		</Attribute>
		<Attribute name="RareShoesDuringEventText" program="program">
			<Program><![CDATA[
                #program;
                var tempString = System::localise('UI_ClawMachine_Rare');
                tempString += toString(System::evaluate('Building::ClawMachine::Object::ClawResult'));
                tempString += '\n';
                tempString += System::localise('UI_PinPals_BowlingShoesPlural');
                return tempString;
            ]]></Program>
		</Attribute>
	</ClawMachine_FormattedText>
	<ExpirationFormattedText>
		<Attribute name="FormattedExpiration24" value="UI_PinPals_24HourExpiry_text" localise="true" parameters="textParams">
			<AttributeSet name="textParams">
				<Attribute name="0" formula="floor(days(System::evaluate('Date::PinPals_End_End') - serverTime(0)))" />
				<Attribute name="1" formula="mod(floor(hours(System::evaluate('Date::PinPals_End') - serverTime(0))), 24)" />
				<Attribute name="2" formula="mod(floor(minutes(System::evaluate('Date::PinPals_End') - serverTime(0))), 60)" />
			</AttributeSet>
		</Attribute>
	</ExpirationFormattedText>
</Attributes>