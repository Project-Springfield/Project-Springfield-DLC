	<NightmarePileManager_New parallel="true">
		<Action type="program">
			<Program><![CDATA[
                    #program;
                    if (User::getVariable('NightmarePileQuestManager_On'))
                    {
                        var characters = Array::create( 'Milhouse',
                        'SherriandTerri',
                        'Martin',
                        'Shauna',
                        'Kearney',
                        'SqueakyVoiceTeen',
                        'Nelson',
                        'Ralph',
                        'Rod',
                        'Todd',
                        'Dolph',
                        'Jimbo');
                        
                        var eligibleCharacters = Array::create();
                        
                        foreach(characters as character)
                        {
                            if (Object::getProperty(Land::getFirstInstanceOfObject(Character::find(character)), 'ID')!=null && User::getVariable('NightmarePileQuestManager_' + character)!=2)
                            {
                                Array::pushBack(eligibleCharacters, character);
                            }
                        }
                        if(Array::size(eligibleCharacters)>0)
                        {
                            User::setVariable('NightmarePileQuestManager_' + Array::getValueAtIndex(eligibleCharacters, rand(Array::size(eligibleCharacters))), 2, true);
                            User::setVariable('NightmarePileQuestManager_On', 0, false);
                        }
                    }
                ]]></Program>
		</Action>
	</NightmarePileManager_New>
