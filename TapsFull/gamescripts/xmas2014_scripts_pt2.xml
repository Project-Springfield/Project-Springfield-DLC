		<Action type="dialogue" character="Homer" text="QST_ProjectAfterParty01_Alt_3_dlg_Start_1_Homer" revealSpeed="medium" />
		<Action type="dialogue" character="Homer" text="QST_ProjectAfterParty01_Alt_3_dlg_Start_2_Homer" revealSpeed="medium" />
