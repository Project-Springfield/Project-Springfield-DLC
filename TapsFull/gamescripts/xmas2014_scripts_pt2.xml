<ScriptPackage>
	<ExitScript>
		<Action type="gotoFrontend" />
	</ExitScript>
	<KillSanta ignoreHUD="true">
		<Action type="deletecharacter" character="Xmas2014_Santa" />
	</KillSanta>
	<WobbleCannonButton parallel="true" cache="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="lessThan(timesToFire,3)" />
				<Requirement type="special currency" currency="FullBottles" amount="1" />
			</Requirements>
			<True>
				<Action type="wobbleSpecialEventButton" buttonID="CannonButtonReady" speed="1.5" angle="10" duration="5s" />
			</True>
		</Action>
	</WobbleCannonButton>
	<Shake parallel="true" cached="true">
		<Action type="sound" sound="103_drop_building.caf" />
		<Action type="setVariable" intVariable="cannonShots" intValue="cannonShots+1" />
		<Action type="floatText">
			<Attribute name="config">
				<AttributeSet name="text">
					<Attribute name="text" value="UI_CannonFiring_FloatText" parameters="messageParams" localise="true">
						<AttributeSet name="messageParams">
							<Attribute name="0" formula="cannonShots" />
						</AttributeSet>
					</Attribute>
					<Attribute name="font" value="eFont_XLBold" />
					<Attribute name="color" value="FF0000FF" />
					<Attribute name="name" value="elfShots" />
					<Attribute name="duration" value="1s" />
					<Attribute name="speed" value="1.2" />
					<Attribute name="resetTime" value="true" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="shakeCamera" length="2" magnitude="10" />
	</Shake>
	<FireElfCannon cached="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="greaterThan(FullBottles,0)" />
			</Requirements>
			<True>
				<Action type="runScript" script="ActualFireElfCannon" package="XMAS2014_Scripts_Pt2" />
			</True>
			<False>
				<Action type="genericMessage">
					<Attribute name="config">
						<AttributeSet name="title">
							<Attribute name="text" value="UI_MiniGame_ElfCannon_errortitle" localise="true" parameters="levelTextData" />
							<Attribute name="font" value="eFont_LargeBold" />
							<Attribute name="color" value="FFD51AFF" />
						</AttributeSet>
						<AttributeSet name="message">
							<Attribute name="text" value="UI_MiniGame_ElfCannon_errormessage" localise="true" />
							<Attribute name="font" value="eFont_Large" />
							<Attribute name="color" value="FFFFFFFF" />
						</AttributeSet>
						<AttributeSet name="button_OK">
							<Attribute name="button_text">
								<AttributeSet name="text">
									<Attribute name="text" value="UI_OK" localise="true" />
								</AttributeSet>
							</Attribute>
						</AttributeSet>
						<AttributeSet name="bg">
							<Attribute name="image" value="ICO_SuperSecretItem" />
						</AttributeSet>
						<AttributeSet name="left">
							<Attribute name="image" value="ico_xmas2014_elfbottleunlock" />
						</AttributeSet>
					</Attribute>
				</Action>
			</False>
		</Action>
	</FireElfCannon>
	<ActualFireElfCannon parallel="true" cached="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="ignoreFire==1" />
			</Requirements>
			<True>
				<Action type="setMiniGameState" name="ElfCannonLaunch" state="Playing" />
			</True>
			<False>
				<Action type="setVariable" intVariable="cannonShots" intValue="1" />
				<Action type="checkRequirements">
					<Requirements>
						<Requirement type="formula" formula="greaterThan(FullBottles,0)" />
					</Requirements>
					<True>
						<Action type="checkRequirements">
							<Requirements>
								<Requirement type="formula" formula="lessThan(timesToFire,3)" />
							</Requirements>
							<True>
								<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_bottle_loaded.caf" />
								<Action type="anim-play" building="ElfCannon" animation="FireCannonIn" />
								<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_fired.caf" replayType="playAnyways" />
								<Action type="sound">
									<Sound sound="sfx_EA_TSTO_elf_woohoo_1.caf" />
									<Sound sound="sfx_EA_TSTO_elf_woohoo_2.caf" />
									<Sound sound="sfx_EA_TSTO_elf_woohoo_3.caf" />
									<Sound sound="sfx_EA_TSTO_elf_woohoo_4.caf" />
									<Sound sound="sfx_EA_TSTO_elf_woohoo_5.caf" />
								</Action>
								<Action type="anim-play" building="ElfCannon" animation="FireCannon" />
								<Action type="anim-play" building="ElfCannon" animation="FireCannonOut" />
								<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_active_out.caf" />
								<Action type="setVariable" intVariable="timesToFire" intValue="timesToFire+1" />
								<Action type="reward" ElfBerries="selector CannonRewardFormula" />
								<Action type="reward" save="true" XMAS2014Score="__XMAS2014_GameConfig:Formulas:CannonCost__" noDrops="true" />
								<Action type="setVariable" intVariable="TotalElvesFired" intValue="TotalElvesFired+System::evaluate('Selector::CannonCostFormula')" />
								<Action type="loseResources" FullBottles="__XMAS2014_GameConfig:Formulas:CannonCost__" />
							</True>
						</Action>
						<Action type="checkRequirements">
							<Requirements>
								<Requirement type="formula" formula="lessThan(timesToFire,3)" />
							</Requirements>
							<False>
								<Action type="setMiniGameState" name="ElfCannonLaunch" state="Ready" />
							</False>
						</Action>
					</True>
					<False>
						<Action type="genericMessage">
							<Attribute name="config">
								<AttributeSet name="title">
									<Attribute name="text" value="UI_MiniGame_ElfCannon_errortitle" localise="true" parameters="levelTextData" />
									<Attribute name="font" value="eFont_LargeBold" />
									<Attribute name="color" value="FFD51AFF" />
								</AttributeSet>
								<AttributeSet name="message">
									<Attribute name="text" value="UI_MiniGame_ElfCannon_errormessage" localise="true" />
									<Attribute name="font" value="eFont_Large" />
									<Attribute name="color" value="FFFFFFFF" />
								</AttributeSet>
								<AttributeSet name="button_OK">
									<Attribute name="button_text">
										<AttributeSet name="text">
											<Attribute name="text" value="UI_OK" localise="true" />
										</AttributeSet>
									</Attribute>
								</AttributeSet>
								<AttributeSet name="bg">
									<Attribute name="image" value="ICO_SuperSecretItem" />
								</AttributeSet>
								<AttributeSet name="left">
									<Attribute name="image" value="ICO_XMAS2014_ElfBottle_LG" />
								</AttributeSet>
							</Attribute>
						</Action>
					</False>
				</Action>
				<Action type="setVariable" intVariable="totalTimesFired" intValue="totalTimesFired+1" />
				<Action type="setVariable" intVariable="currentTimesFired" intValue="currentTimesFired+1" />
			</False>
		</Action>
		<Action type="setVariable" intVariable="ignoreFire" intValue="0" />
	</ActualFireElfCannon>
	<ResetElfCannon parallel="true" ignoreHud="true">
		<Action type="setVariable" intVariable="timesToFire" intValue="0" />
		<Action type="setVariable" intVariable="ignoreFire" intValue="1" />
		<Action type="checkRequirements">
			<Requirement type="formula" formula="greaterThan(FullBottles,0)" />
			<True>
				<Action type="collectIndicator" building="ElfCannon" />
			</True>
		</Action>
	</ResetElfCannon>
	<GoToElfCannon>
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="gotoState" state="GameState_InGame" />
				<Action type="pantoobject" building="ElfCannon" fakeTouch="false" />
			</True>
			<False>
				<Action type="userConfirmation" text="UI_TravelToSpringfieldForQuest" title="BLD_KrustylandShuttleToSpringfield_name" var="confirm_gotoSprinfield" />
				<Action type="checkExpression" expression="confirm_gotoSprinfield">
					<ExpressionCase value="1">
						<Action type="gotoland" land="Springfield" />
					</ExpressionCase>
				</Action>
			</False>
		</Action>
	</GoToElfCannon>
	<GoToElfCannonControl>
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="gotoState" state="GameState_InGame" />
				<Action type="pantoobject" building="ElfCannonControl" fakeTouch="true" fakeTouchAvoidsIndicators="true" />
			</True>
			<False>
				<Action type="userConfirmation" text="UI_TravelToSpringfieldForQuest" title="BLD_KrustylandShuttleToSpringfield_name" var="confirm_gotoSprinfield" />
				<Action type="checkExpression" expression="confirm_gotoSprinfield">
					<ExpressionCase value="1">
						<Action type="gotoland" land="Springfield" />
					</ExpressionCase>
				</Action>
			</False>
		</Action>
	</GoToElfCannonControl>
	<OnFriendTouchesElfCannon parallel="true" cached="true">
		<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_bottle_loaded.caf" />
		<Action type="anim-play" building="ElfCannon" animation="FireCannonIn" />
		<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_fired.caf" />
		<Action type="sound">
			<Sound sound="sfx_EA_TSTO_elf_woohoo_1.caf" />
			<Sound sound="sfx_EA_TSTO_elf_woohoo_2.caf" />
			<Sound sound="sfx_EA_TSTO_elf_woohoo_3.caf" />
			<Sound sound="sfx_EA_TSTO_elf_woohoo_4.caf" />
			<Sound sound="sfx_EA_TSTO_elf_woohoo_5.caf" />
		</Action>
		<Action type="anim-play" onBuilding="ElfCannon" animation="FireCannonFromFriendAction" />
		<Action type="anim-play" onBuilding="ElfCannon" animation="FireCannonOut" />
		<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_active_out.caf" />
	</OnFriendTouchesElfCannon>
	<ShakeFromFriendAction parallel="true" cached="true">
		<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_fired.caf" replayType="playAnyways" />
		<Action type="shakeCamera" length="2" magnitude="10" />
	</ShakeFromFriendAction>
	<XMASFriendsHUDInfo parallel="true">
		<Action type="runScript" script="XMASFriendsHUDInfoAux" package="XMAS2014_Scripts_Pt2" />
	</XMASFriendsHUDInfo>
	<XMASFriendsHUDInfoAux>
		<Action type="message" text="UI_XMAS2014_Pt2_FriendMapHelp_text" title="UI_XMAS2014_title" />
	</XMASFriendsHUDInfoAux>
	<OnWorldMapLoad parallel="true">
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="variable" variable="ShowLeaderboard" />
				<Requirement type="variable" variable="OpenLeaderboard" />
				<Requirement type="config" key="XMAS2014_GameConfig_Pt2:Enable:LeaderboardPopup" value="1" />
			</Requirements>
			<True>
				<Action type="setVariable" boolVariable="OpenLeaderboard" boolValue="false" />
				<Action type="gotoState" state="WorldState_Leaderboard">
					<Extras>
						<Attribute name="MenuConfig" value="XMAS2014_LeaderboardMenuConfig_Pt2:LeaderboardMenu" />
					</Extras>
				</Action>
			</True>
		</Action>
	</OnWorldMapLoad>
	<UpdateGunPowderBalance>
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="land" land="Springfield" />
				<Requirement type="friend land" not="true" />
			</Requirements>
			<True>
				<Action type="setVariable" intVariable="GunPowderBalance" intValue="System::evaluate('Spendable::GunPowder')" />
			</True>
		</Action>
	</UpdateGunPowderBalance>
	<OnFriendButtonPressed>
		<Action type="setVariable" boolVariable="OpenLeaderboard" boolValue="true" />
	</OnFriendButtonPressed>
	<CannonButtonPressed ignoreHUD="true">
		<Action type="pantoobject" building="ElfCannon" fakeTouch="false" />
	</CannonButtonPressed>
	<ElfZapperButtonPressed>
		<Action type="notification" title="UI_ElflectricSwatter_name" text="UI_ElfSwatter_SystemMessage" button="UI_OK" icon="ICO_MHUD_XMAS14_Teleporter_SE" />
	</ElfZapperButtonPressed>
	<SpecialEventButtonInFriendMapPressed>
		<Action type="gotoState" state="WorldState_Leaderboard">
			<Extras>
				<Attribute name="MenuConfig" value="XMAS2014_LeaderboardMenuConfig_Pt2:LeaderboardMenu" />
			</Extras>
		</Action>
	</SpecialEventButtonInFriendMapPressed>
	<StunBadElf parallel="true" cache="true">
		<Action type="runScript" script="BadElfStunnedReward" package="XMAS2014_Scripts_Pt2" waitUntilComplete="true" />
		<Action type="setVariable" intVariable="XMAS2014StunnedBadElves" intValue="XMAS2014StunnedBadElves+1" />
		<Action type="delay" time="rand(10)/100" />
		<Action type="stop" character="Xmas014_BadElf" />
		<Action type="sound" sound="364_bunny_electrocuted.caf" />
		<Action type="playanimation" character="Xmas014_BadElf" animation="Teleport" />
		<Action type="deletecharacter" character="Xmas014_BadElf" />
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="character count" name="Xmas014_BadElf" quantity="1" not="true" />
			</Requirements>
			<True>
				<Action type="message" text="UI_ElvesCleared_SystemMessage" dismissOnOKOnly="false" />
			</True>
		</Action>
	</StunBadElf>
	<ActivateBadElfStunner parallel="true">
		<Action type="vacuum" config="BadElfStunner" fromSpecialEvent="XMAS2014_Pt2" />
	</ActivateBadElfStunner>
	<DoBadElfStun parallel="true" exclusive="true">
		<Action type="runScript" script="StunBadElf" package="XMAS2014_Scripts_Pt2" waitUntilComplete="true" />
	</DoBadElfStun>
	<BadElfTappedReward parallel="true" cache="true">
		<Action type="rewardchoice">
			<Reward chance="__XMAS2014_GameConfig:ElfRewards:ElfBottle_Chance__" chanceType="s" FullBottles="__XMAS2014_GameConfig:ElfRewards:ElfBottle_Num__" />
		</Action>
		<Action type="checkRequirements">
			<Requirement type="building" building="ElfCannonControl" />
			<True>
				<Action type="rewardchoice">
					<Reward chance="__XMAS2014_GameConfig:ElfRewards:CannonParts_Chance__" chanceType="s" GunPowder="__XMAS2014_GameConfig:ElfRewards:CannonParts_Num__" />
				</Action>
			</True>
		</Action>
	</BadElfTappedReward>
	<BadElfStunnedReward parallel="true" cache="true">
		<Action type="rewardchoice">
			<Reward chance="__XMAS2014_GameConfig:ElfRewards:ElfBottle_Chance__" chanceType="s" FullBottles="__XMAS2014_GameConfig:ElfRewards:ElfBottle_Num__" />
		</Action>
		<Action type="checkRequirements">
			<Requirement type="building" building="ElfCannonControl" />
			<True>
				<Action type="rewardchoice">
					<Reward chance="__XMAS2014_GameConfig:ElfRewards:CannonParts_Chance__" chanceType="s" GunPowder="__XMAS2014_GameConfig:ElfRewards:CannonParts_Num__" />
				</Action>
			</True>
		</Action>
	</BadElfStunnedReward>
	<SpawnElvesFromCannon cache="true">
		<Action type="recieveMultipleFriendScripts" scriptToRun="XMAS2014_Scripts_Pt2:SpawnElfFromCannon" />
	</SpawnElvesFromCannon>
	<SpawnStartElves>
		<Action type="createobject" name="Xmas014_BadElf" count="30" save="true" panCamera="false" />
	</SpawnStartElves>
	<SpawnReplacementForElves cache="true" ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="formula" formula="SOTElfSessionStartTime==0" />
			<True>
				<Action type="setVariable" dateVariable="SOTElfSessionStartTime" />
			</True>
		</Action>
		<Action type="setVariable" dateVariable="SOTElfSessionCurrentTime" />
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="lessThan(SOTElfSessionCurrentTime - SOTElfSessionStartTime, 10)" />
			</Requirements>
			<True>
				<Action type="checkRequirements">
					<Requirements>
						<Requirement type="formula" formula="lessThan(SOTElfSessionSpawnCount, 6)" />
					</Requirements>
					<True>
						<Action type="setVariable" intVariable="SOTElfSpawnCloseToCannon" intValue="1" />
						<Action type="setVariable" intVariable="SOTElfEffectEnabled" intValue="1" />
					</True>
					<False>
						<Action type="setVariable" intVariable="SOTElfSpawnCloseToCannon" intValue="0" />
						<Action type="setVariable" intVariable="SOTElfEffectEnabled" intValue="0" />
					</False>
				</Action>
			</True>
			<False>
				<Action type="setVariable" intVariable="SOTElfSpawnCloseToCannon" intValue="0" />
				<Action type="setVariable" intVariable="SOTElfEffectEnabled" intValue="1" />
			</False>
		</Action>
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="SOTElfSpawnCloseToCannon==1" />
			</Requirements>
			<True>
				<Action type="createobject" name="Xmas014_BadElf" count="1" save="true" panCamera="false" building="ElfCannon" awayFromSpawnLoc="200.0f" prefAwayGridType="grass" enableCharacterOverlay="true" />
				<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_bottle_broken.caf" />
				<Action type="setVariable" intVariable="SOTElfSessionSpawnCount" intValue="SOTElfSessionSpawnCount+1" />
			</True>
			<False>
				<Action type="checkRequirements">
					<Requirements>
						<Requirement type="formula" formula="SOTElfEffectEnabled==1" />
					</Requirements>
					<True>
						<Action type="createobject" name="Xmas014_BadElf" count="1" save="true" panCamera="false" enableCharacterOverlay="true" />
						<Action type="sound" sound="sfx_EA_TSTO_elf_cannon_bottle_broken.caf" />
					</True>
					<False>
						<Action type="createobject" name="Xmas014_BadElf" count="1" save="true" panCamera="false" />
					</False>
				</Action>
			</False>
		</Action>
	</SpawnReplacementForElves>
	<ResetSOTElfSessionStartTime>
		<Action type="setVariable" dateVariable="SOTElfSessionStartTime" />
	</ResetSOTElfSessionStartTime>
	<SpawnElfFromCannon parallel="true" cache="true">
		<Action type="createobject" name="Xmas014_BadElf" count="1" save="true" building="SimpsonHome" awayFromSpawnLoc="200.0f" />
	</SpawnElfFromCannon>
	<BadElf_Tapped parallel="true">
		<Action type="runScript" script="BadElfTappedReward" package="XMAS2014_Scripts_Pt2" waitUntilComplete="true" />
		<Action type="sound">
			<Sound sound="sfx_EA_TSTO_elf_scared_1.caf" />
			<Sound sound="sfx_EA_TSTO_elf_scared_2.caf" />
			<Sound sound="sfx_EA_TSTO_elf_scared_3.caf" />
		</Action>
		<Action type="playanimation" character="Xmas014_BadElf" animation="Gets_Squashed" />
		<Action type="fade" character="Xmas014_BadElf" fade="outmax" />
		<Action type="deletecharacter" character="Xmas014_BadElf" />
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="character count" name="Xmas014_BadElf" quantity="1" not="true" />
			</Requirements>
			<True>
				<Action type="message" text="UI_ElvesCleared_SystemMessage" dismissOnOKOnly="false" />
			</True>
		</Action>
	</BadElf_Tapped>
