			<AttributeSet name="StartButton">
				<Attribute name="script" value="StateOfDespair_Scripts:SkyFingerMonument_Start" />
			</AttributeSet>
			<AttributeSet name="CooldownRushButton">
				<Attribute name="script" value="StateOfDespair_Scripts:SkyFingerMonument_Rush" />
			</AttributeSet>
		</Attribute>
	</SkyFingerMonument_StatusMenuComponentButtonConfig>
