	<RewardPrize>
		<Action type="checkRequirements" formatMe="deep">
			<Requirement type="conditional" logic="OR">
				<Requirement type="formula" formula="{TaskStartTime}==0" />
				<Requirement type="relative time" baseTime="{TaskStartTime}" defaultTime="HomerballActOne" endOffset="1d" />
			</Requirement>
			<True>
				<Action type="runScript" script="RewardMessage" package="June2015_QuestLotScripts">
					<FormatToken key="UnlockText" value="{UnlockText}" />
					<FormatToken key="MerchandiseBonusAward" value="{DailyTaskBonusRewardAmount}" />
					<FormatToken key="ImageName" value="{ImageName}" />
				</Action>
				<Action type="reward" HomerballActivePrizeTrackCurrency="{DailyTaskBonusRewardAmount}" />
			</True>
			<False>
				<Action type="runScript" script="RewardMessage" package="June2015_QuestLotScripts">
					<FormatToken key="UnlockText" value="{UnlockText}" />
					<FormatToken key="MerchandiseBonusAward" value="0" />
					<FormatToken key="ImageName" value="{ImageName}" />
				</Action>
			</False>
		</Action>
		<Action type="reward" formatMe="deep">
			<Item type="{ItemType}" name="{ItemName}" useImmediately="true" />
		</Action>
	</RewardPrize>
	<RewardMessage>
		<Action type="genericMessage" formatMe="deep">
			<Attribute name="config">
				<AttributeSet name="underlinedBranch">
					<Attribute name="active" value="true" />
				</AttributeSet>
				<AttributeSet name="underlinedTitle">
					<Attribute name="text" value="UI_June2015_DailyPrize_Title" localise="true" />
					<Attribute name="font" value="eFont_LargeBold" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="message">
					<Attribute name="text" value="{UnlockText}" localise="true" />
					<Attribute name="font" value="eFont_Normal" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="extraRewardBranch">
					<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					<Attribute name="active" value="true" />
				</AttributeSet>
				<AttributeSet name="extraRewardText">
					<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					<Attribute name="text" value="UI_June2015_DailyPrize_Bonus" localise="true" />
					<Attribute name="font" value="eFont_Normal" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="extraReward2_Amount">
					<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					<Attribute name="text" value="{MerchandiseBonusAward}" />
					<Attribute name="font" value="eFont_NormalBold" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="extraReward2_Image">
					<Requirements>
						<Requirement type="time" end="HomerballActTwo" />
						<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					</Requirements>
					<Attribute name="image" value="ICO_June2015_FoamFinger_SM" />
				</AttributeSet>
				<AttributeSet name="extraReward2_Image">
					<Requirements>
						<Requirement type="time" start="HomerballActTwo" end="HomerballActThree" />
						<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					</Requirements>
					<Attribute name="image" value="ICO_June2015_Hat_SM" />
				</AttributeSet>
				<AttributeSet name="extraReward2_Image">
					<Requirements>
						<Requirement type="time" start="HomerballActThree" />
						<Requirement type="formula" formula="greaterThan({MerchandiseBonusAward},0)" />
					</Requirements>
					<Attribute name="image" value="ICO_June2015_Pennant_SM" />
				</AttributeSet>
				<AttributeSet name="largeImage">
					<Attribute name="image" value="{ImageName}" />
				</AttributeSet>
				<AttributeSet name="button_OK">
					<Attribute name="button_text">
						<AttributeSet name="text">
							<Attribute name="text" value="UI_Collect" localise="true" />
						</AttributeSet>
					</Attribute>
				</AttributeSet>
			</Attribute>
		</Action>
	</RewardMessage>
