					<Requirement type="active quest" quest="Plot_WallStreet_Master" />
					<Program><![CDATA[
                            #program;
                            var maxStrength = toDouble(System::getConfigValue('__SecretAgents_GameConfig:Plots:WallStreet_Total__'));
                            var completedPlots = User::getVariable('Plot_WallStreet_CompleteCMs');
                            if (completedPlots >= maxStrength)
                            {
                            return 1;
                            }
                            else
                            {
                            return completedPlots/maxStrength;
                            }
                        ]]></Program>
				</Attribute>
				<Attribute name="fill" program="program">
					<Requirements>
						<Requirement type="active quest" quest="Plot_WallStreet_Master" not="true" />
					</Requirements>
					<Program><![CDATA[
                            #program;
                            return 1;
                        ]]></Program>
				</Attribute>
				<Attribute name="color" value="0x00cc25FF" />
				<Attribute name="reverse" value="false" />
			</AttributeSet>
			<AttributeSet name="IfStatusCompleted">
				<Attribute name="active" value="false" />
			</AttributeSet>
		</Attribute>
