	</DoggySmithers_Tapped>
	<HoloTheater_Tapped parallel="true">
		<Action type="checkExpression" expression="Object::State">
			<ExpressionCase value="0">
				<Action type="anim-play" building="HoloTheater" animation="Active_In" />
				<Action type="sound" sound="HoloTheater_Tap" />
			</ExpressionCase>
			<ExpressionCase value="1">
				<Action type="anim-play" building="HoloTheater" animation="Active_Out" />
				<Action type="sound" sound="HoloTheater_Tap" />
			</ExpressionCase>
		</Action>
		<Action type="setObjectVariable" variable="State" value="mod(Object::State+1, 2)" asExpression="true" allowOrphan="false" />
	</HoloTheater_Tapped>
	<CrazyPlane_Tapped parallel="true">
		<Action type="anim-play" animation="ActiveNoLoop" />
	</CrazyPlane_Tapped>
	<MechRobot_Tapped parallel="true">
		<Action type="if">
			<If expression="System::evaluate('Job::RideTheMech::Active')!=1">
				<Then>
					<Action type="anim-play" animation="ActiveNoLoop" />
					<Action type="setObjectVariable" variable="MechRobotActive" value="0" allowOrphan="false" />
				</Then>
			</If>
		</Action>
	</MechRobot_Tapped>
