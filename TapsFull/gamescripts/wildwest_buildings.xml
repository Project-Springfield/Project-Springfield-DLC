		</StatusMenuReorder>
		<ScriptTrigger title="UI_WildWestFinale_ScriptTriggerName" description="UI_WildWestFinale_ScriptTriggerDesc" buttonLabel="UI_GoTo" buttonColor="0091F6FF">
			<Requirements>
				<Requirement type="conditional" logic="OR">
					<Requirement type="variable" variable="WildWest_Scene1" />
					<Requirement type="variable" variable="WildWest_Scene2" />
					<Requirement type="variable" variable="WildWest_Scene3" />
					<Requirement type="variable" variable="WildWest2016_CollectedAct1Scene" />
					<Requirement type="variable" variable="WildWest2016_CollectedAct2Scene" />
					<Requirement type="variable" variable="WildWest2016_CollectedAct3Scene" />
				</Requirement>
			</Requirements>
			<Script onButtonPressed="WildWest_Scripts:TabPressed_Scenes" />
		</ScriptTrigger>
		<SoundKit event="BuildingTouched" name="Town_Plaza_State" />
		<OnEvent event="BuildingPlaced" onObject="TownPlaza" user="local">
			<Requirement type="variable" variable="NorthwardBound_TownPlazaUpgrades" />
			<Script name="TownPlaza_Init" package="NorthwardBound_Scripts" />
		</OnEvent>
		<EffectOverlays>
			<EffectOverlay bsv3="TownPlaza_InstigateLawlessness" anim="Active01" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="and(System::evaluate('Job::WW_InstigateChaos_Dual::Active'), WildWest_ScenePlaying == 0)" />
			<EffectOverlay bsv3="TownPlaza_OminouslyWarn" anim="Active" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="and(System::evaluate('Job::WW_OminouslyWarn_Dual::Active'), WildWest_ScenePlaying == 0)" />
			<EffectOverlay bsv3="TownPlaza_StageCoach" anim="Part01" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="false" registerVariableEvent="true" formulaRequirement="WildWest_Scene1Switch == 1" />
			<EffectOverlay bsv3="TownPlaza_StageCoach" anim="Part02" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="false" registerVariableEvent="true" formulaRequirement="WildWest_Scene1Switch == 2" />
			<EffectOverlay bsv3="TownPlaza_Faceoff" anim="Part01" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="WildWest_Scene2Switch == 1" />
			<EffectOverlay bsv3="TownPlaza_Faceoff" anim="Part02" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="WildWest_Scene2Switch == 2" />
			<EffectOverlay bsv3="TownPlaza_Faceoff" anim="Part03" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="WildWest_Scene2Switch == 3" />
			<EffectOverlay bsv3="TownPlaza_DefendTheMission" flippedbsv3="TownPlaza_DefendTheMissionFlipped" anim="Part01" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="false" registerVariableEvent="true" formulaRequirement="WildWest_Scene3Switch == 1" />
			<EffectOverlay bsv3="TownPlaza_DefendTheMission" flippedbsv3="TownPlaza_DefendTheMissionFlipped" anim="Part02" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="WildWest_Scene3Switch == 2" />
			<EffectOverlay bsv3="TownPlaza_DefendTheMission" flippedbsv3="TownPlaza_DefendTheMissionFlipped" anim="Part03" randomStartFrame="false" rooted="rooted" allowInNormal="true" allowInWrecked="false" animLoop="true" registerVariableEvent="true" formulaRequirement="WildWest_Scene3Switch == 3" />
