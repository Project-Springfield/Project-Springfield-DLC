<SpecialEvents>
	<SpecialEvent id="126000" name="MuscularMarge_PromoEvent" character="PromoGilPermanent">
		<OnEvent event="LandVisited" user="local" ignoreTypeCheck="true">
			<Requirements>
				<Requirement type="active quest" quest="MuscularMarge_Quest" />
			</Requirements>
			<Script name="MuscularMarge_Recommend" package="MuscularMarge_PromoScripts" />
		</OnEvent>
		<Variable name="MuscularMarge_INDICATOR_ACTIVE" value="true" />
		<Variable name="PROMO_STARTED" value="true" />
		<Variable name="PROMO_SHOW_IN_BUILD_MENU" value="false" />
		<Variable name="Promo_MuscularMarge_Active" value="true" />
		<Promo eventName="MuscularMarge_PromoEvent" questID="126900" type="building" object="ShapesGym" file="Promo_MuscularMarge" yes="BTN_175Donuts" visualEndDate="MuscularMarge_End" />
	</SpecialEvent>
	<SpecialEvent id="126001" name="MuscularMarge_PostSkinReward">
		<Buildings override="true">
			<Building name="ShapesGym">
				<StorePositioningRule rule="ShapesGymRule_PostSkinReward" />
			</Building>
		</Buildings>
	</SpecialEvent>
</SpecialEvents>