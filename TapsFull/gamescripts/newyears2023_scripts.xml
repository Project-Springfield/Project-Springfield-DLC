<ScriptPackage>
	<ShowEventIntro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="NewYears2023MiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="NewYears2023_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="NewYears2023_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="NewYears2023_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="NewYears2023_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_NewYears2023_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="timeLeft">
					<Attribute name="text" value="UI_NewYears2023_IntroPanelFooter" localise="true" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="textColumn1">
					<Attribute name="text" value="UI_NewYears2023_IntroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn2">
					<Attribute name="text" value="UI_NewYears2023_IntroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_NewYears2023_MiniEventPanel_Intro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="setVariable" boolVariable="NewYears2023_IntroSeen" boolValue="true" />
		<Action type="updateVariable" variable="NewYears2023_Store" value="true" />
		<Action type="recommendation" recType="Event" name="NewYears2023_SpringfieldsGotTalent_OnePanel" eventName="UI_NewYears2023_Title" limitedTime="true" limitedTimeEndDate="NewYears2023_End" />
		<Action type="reparseBuildMenu" />
	</ShowEventIntro>
	<ShowEventOutro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="NewYears2023MiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_NewYears2023_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="textColumn4">
					<Attribute name="text" value="UI_NewYears2023_OutroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn5">
					<Attribute name="text" value="UI_NewYears2023_OutroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_NewYears2023_MiniEventPanel_Outro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="notification" title="CON_ShatteredDreamsMysteryBox_name" button="UI_OK" text="UI_ShatteredDreams_notif" icon="ICO_STOR_Single_ShatteredDreams_MysteryBox" />
		<Action type="reward" save="true" ShatteredToken="1" />
		<Action type="recommendation" recType="Promoted" name="ShatteredDreamsMysteryBox_TimeTravel" limitedTime="true" limitedTimeEndDate="NewYears2023_ShatteredDreams_End" />
		<Action type="reparseBuildMenu" />
		<Action type="consumable" consumable="ShatteredDreamsMysteryBox_TimeTravel" fromInventory="false" />
	</ShowEventOutro>
	<ExpiryWarning_72hr>
		<Action type="message" text="UI_NewYears2023_72HourExpiry_text" />
		<Action type="setVariable" boolVariable="NewYears2023_PreExpiryDisplayed72" boolValue="true" />
		<Action type="runScript" script="NewYears2023_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_72hr>
	<ExpiryWarning_24hr>
		<Action type="message" text="{FormattedExpiration24}" formatMe="root">
			<FormattingConfig path="NewYears2023_GameConfig:ExpirationFormattedText" />
		</Action>
		<Action type="setVariable" boolVariable="NewYears2023_PreExpiryDisplayed24" boolValue="true" />
		<Action type="runScript" script="NewYears2023_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_24hr>
	<DisplayExpiry>
		<Action type="message" text="UI_NewYears2023_Expired_text" />
		<Action type="clearRecommendations" category="Event" name="NewYears2023_OnePanel" />
		<Action type="runScript" script="NewYears2023_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</DisplayExpiry>
	<EventCompleted>
		<Action type="setVariable" boolVariable="NewYears2023_Complete" boolValue="true" />
	</EventCompleted>
	<OpenMasterQuest>
		<Action type="checkRequirements">
			<Requirement type="gamestate" state="GameState_QuestProgress" />
			<True>
				<Action type="changeQuestForProgressState" name="NewYears2023_MasterQuest" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_QuestProgress" forceState="true">
					<Extras>
						<Attribute name="questParentDelegate" value="NewYears2023_MasterQuest" />
					</Extras>
				</Action>
			</False>
		</Action>
	</OpenMasterQuest>
	<ShowJobList ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="if">
					<If>
						<Requirement type="active quest" quest="SpringfieldIdle_1" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="NewYears2023_Event" />
									<Attribute name="JobList" value="NewYears2023_JL_1" />
								</Extras>
							</Action>
						</Then>
					</If>
					<ElseIf>
						<Requirement type="active quest" quest="SpringfieldIdle_2" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="NewYears2023_Event" />
									<Attribute name="JobList" value="NewYears2023_JL_2" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="SpringfieldIdle_3" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="NewYears2023_Event" />
									<Attribute name="JobList" value="NewYears2023_JL_3" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="SpringfieldIdle_4" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="NewYears2023_Event" />
									<Attribute name="JobList" value="NewYears2023_JL_4" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="SpringfieldIdle_5" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="NewYears2023_Event" />
									<Attribute name="JobList" value="NewYears2023_JL_5" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
				</Action>
			</True>
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</ShowJobList>
	<CheckIfEnd ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="quest" quest="NewYears2023_EventExpired" />
			<True>
				<Action type="setVariable" boolVariable="NewYears2023_Expired" boolValue="true" />
			</True>
		</Action>
	</CheckIfEnd>
	<NewYears2023TokenForDozenDonuts>
		<Action type="notification" title="MTX_DozenDonuts_name" text="MTX_DozenDonuts_name" icon="ICO_Thanksgiving2018_DozenDonutsPopup_LG" button="UI_OK" />
		<Action type="reward" save="true" donuts="12" />
		<Action type="checkRequirements">
			<Requirement type="currency" NewYears2023MysteryBoxToken="1" />
			<True>
				<Action type="runScript" script="NewYears2023_Scripts:GoToNewYears2023TokenExchange" />
			</True>
		</Action>
	</NewYears2023TokenForDozenDonuts>
	<GoToNewYears2023TokenExchange>
		<Action type="focusOnMenuItem" itemName="NewYears2023TokenForDozenDonuts" menuCategory="SpringfieldsGotTalent_Category" />
	</GoToNewYears2023TokenExchange>
	<RecommendStoreCategory_SpringfieldsGotTalent ignoreHUD="true">
		<Action type="reparseBuildMenu" />
		<Action type="clearRecommendations" category="Event" name="NewYears2023_SpringfieldsGotTalent_OnePanel" />
		<Action type="recommendation" recType="Event" name="NewYears2023_SpringfieldsGotTalent_OnePanel" eventName="UI_NewYears2023_Title" limitedTime="true" limitedTimeEndDate="NewYears2023_End" />
		<Action type="focusOnMenuItem" menuCategory="FrontPage" />
		<Action type="reparseBuildMenu" />
	</RecommendStoreCategory_SpringfieldsGotTalent>
	<EnterStoreCategory_SpringfieldsGotTalent ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="SpringfieldsGotTalent_Category" />
	</EnterStoreCategory_SpringfieldsGotTalent>
	<NewYears2023_MTX_DisplayPromo ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="Currency" itemName="NewYears2023_132DonutsDerZipZorp" />
	</NewYears2023_MTX_DisplayPromo>
</ScriptPackage>