<ScriptPackage>
	<ShowEventIntro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="WhenTheBoughBreaksMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="WhenTheBoughBreaks_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="WhenTheBoughBreaks_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="WhenTheBoughBreaks_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="WhenTheBoughBreaks_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="timeLeft">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_IntroPanelFooter" localise="true" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="textColumn1">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_IntroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn2">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_IntroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_WhenTheBoughBreaks_MiniEventPanel_Intro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="setVariable" boolVariable="WhenTheBoughBreaks_IntroSeen" boolValue="true" />
		<Action type="updateVariable" variable="WhenTheBoughBreaks_Store" value="true" />
		<Action type="recommendation" recType="Event" name="WhenTheBoughBreaks_WhenTheBoughBreaks_OnePanel" eventName="UI_WhenTheBoughBreaks_Title" limitedTime="true" limitedTimeEndDate="WhenTheBoughBreaks_End" />
		<Action type="reparseBuildMenu" />
	</ShowEventIntro>
	<ShowEventOutro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="WhenTheBoughBreaksMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="textColumn4">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_OutroPanelText_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn5">
					<Attribute name="text" value="UI_WhenTheBoughBreaks_OutroPanelText_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_WhenTheBoughBreaks_MiniEventPanel_Outro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="notificationLarge" title="CON_ShatteredDreamsMysteryBox_name" button="UI_OK" text="UI_ShatteredDreams_notif" icon="ICO_STOR_Single_ShatteredDreams_MysteryBox" />
		<Action type="reward" save="true" ShatteredToken="1" />
		<Action type="recommendation" recType="Promoted" name="ShatteredDreamsMysteryBox_Fantasy" limitedTime="true" limitedTimeEndDate="WhenTheBoughBreaks_ShatteredDreams_End" />
		<Action type="reparseBuildMenu" />
		<Action type="consumable" consumable="ShatteredDreamsMysteryBox_Fantasy" fromInventory="false" />
	</ShowEventOutro>
	<ExpiryWarning_72hr>
		<Action type="message" text="UI_WhenTheBoughBreaks_72HourExpiry_text" />
		<Action type="setVariable" boolVariable="WhenTheBoughBreaks_PreExpiryDisplayed72" boolValue="true" />
		<Action type="runScript" script="WhenTheBoughBreaks_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_72hr>
	<ExpiryWarning_24hr>
		<Action type="message" text="{FormattedExpiration24}" formatMe="root">
			<FormattingConfig path="WhenTheBoughBreaks_GameConfig:ExpirationFormattedText" />
		</Action>
		<Action type="setVariable" boolVariable="WhenTheBoughBreaks_PreExpiryDisplayed24" boolValue="true" />
		<Action type="runScript" script="WhenTheBoughBreaks_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_24hr>
	<DisplayExpiry>
		<Action type="message" text="UI_WhenTheBoughBreaks_Expired_text" />
		<Action type="clearRecommendations" category="Event" name="WhenTheBoughBreaks_OnePanel" />
		<Action type="runScript" script="WhenTheBoughBreaks_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</DisplayExpiry>
	<EventCompleted>
		<Action type="setVariable" boolVariable="WhenTheBoughBreaks_Complete" boolValue="true" />
	</EventCompleted>
	<OpenMasterQuest>
		<Action type="checkRequirements">
			<Requirement type="gamestate" state="GameState_QuestProgress" />
			<True>
				<Action type="changeQuestForProgressState" name="WhenTheBoughBreaks_MasterQuest" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_QuestProgress" forceState="true">
					<Extras>
						<Attribute name="questParentDelegate" value="WhenTheBoughBreaks_MasterQuest" />
					</Extras>
				</Action>
			</False>
		</Action>
	</OpenMasterQuest>
	<ShowJobList ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="if">
					<If>
						<Requirement type="active quest" quest="ThreesACrowd_1" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="WhenTheBoughBreaks_Event" />
									<Attribute name="JobList" value="WhenTheBoughBreaks_JL_1" />
								</Extras>
							</Action>
						</Then>
					</If>
					<ElseIf>
						<Requirement type="active quest" quest="ThreesACrowd_2" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="WhenTheBoughBreaks_Event" />
									<Attribute name="JobList" value="WhenTheBoughBreaks_JL_2" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ThreesACrowd_3" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="WhenTheBoughBreaks_Event" />
									<Attribute name="JobList" value="WhenTheBoughBreaks_JL_3" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ThreesACrowd_4" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="WhenTheBoughBreaks_Event" />
									<Attribute name="JobList" value="WhenTheBoughBreaks_JL_4" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="ThreesACrowd_5" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="WhenTheBoughBreaks_Event" />
									<Attribute name="JobList" value="WhenTheBoughBreaks_JL_5" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
				</Action>
			</True>
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</ShowJobList>
	<CheckIfEnd ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="quest" quest="WhenTheBoughBreaks_EventExpired" />
			<True>
				<Action type="setVariable" boolVariable="WhenTheBoughBreaks_Expired" boolValue="true" />
			</True>
		</Action>
	</CheckIfEnd>
	<AutoApply_BigBabyBarneySkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="WhenTheBoughBreaks_Scripts:AutoApply_BigBabyBarneySkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_BigBabyBarneySkin_End>
	<AutoApply_BigBabyBarneySkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="BigBabyBarney" fromInventory="true" />
	</AutoApply_BigBabyBarneySkin_Delayed>
	<RecommendStoreCategory_WhenTheBoughBreaks ignoreHUD="true">
		<Action type="reparseBuildMenu" />
		<Action type="clearRecommendations" category="Event" name="WhenTheBoughBreaks_WhenTheBoughBreaks_OnePanel" />
		<Action type="recommendation" recType="Event" name="WhenTheBoughBreaks_WhenTheBoughBreaks_OnePanel" eventName="UI_WhenTheBoughBreaks_Title" limitedTime="true" limitedTimeEndDate="WhenTheBoughBreaks_End" />
		<Action type="focusOnMenuItem" menuCategory="FrontPage" />
		<Action type="reparseBuildMenu" />
	</RecommendStoreCategory_WhenTheBoughBreaks>
	<EnterStoreCategory_WhenTheBoughBreaks ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="WhenTheBoughBreaks_Category" />
	</EnterStoreCategory_WhenTheBoughBreaks>
	<WhenTheBoughBreaks_MTX_DisplayPromo ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="Currency" itemName="WhenTheBoughBreaks_TrayOf132Donuts" />
	</WhenTheBoughBreaks_MTX_DisplayPromo>
</ScriptPackage>