<ScriptPackage>
	<ShowEventIntro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="KrustysLastGaspOnlineMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="KrustysLastGaspOnline_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="KrustysLastGaspOnline_IntroSeen" not="true" />
					<Attribute name="text" value="UI_MiniEvents_PlayButton" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Requirement type="variable" variable="KrustysLastGaspOnline_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Requirement type="variable" variable="KrustysLastGaspOnline_IntroSeen" />
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="timeLeft">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_IntroPanel_Timer" localise="true" />
					<Attribute name="color" value="FFFFFFFF" />
				</AttributeSet>
				<AttributeSet name="textColumn1">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_IntroPanel_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn2">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_IntroPanel_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_KrustysLastGaspOnline_MiniEventPanel_Intro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
		<Action type="setVariable" boolVariable="KrustysLastGaspOnline_IntroSeen" boolValue="true" />
		<Action type="updateVariable" variable="KrustysLastGaspOnline_Store" value="true" />
		<Action type="recommendation" recType="Event" name="KrustysLastGaspOnline_OnePanel" eventName="UI_KrustysLastGaspOnline_Title" limitedTime="true" limitedTimeEndDate="KrustysLastGaspOnline_End" />
		<Action type="reparseBuildMenu" />
	</ShowEventIntro>
	<ShowEventOutro>
		<Action type="miniEventPanel">
			<Attribute name="config">
				<AttributeSet name="source">
					<Attribute name="file" value="KrustysLastGaspOnlineMiniEventPopUp" />
				</AttributeSet>
				<AttributeSet name="playButtonText">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="playButtonTextHL">
					<Attribute name="text" value="UI_OK" localise="true" />
				</AttributeSet>
				<AttributeSet name="titleText">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_Title" localise="true" />
					<Attribute name="color" value="ffcb1bff" />
				</AttributeSet>
				<AttributeSet name="textColumn4">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_OutroPanel_1" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="textColumn5">
					<Attribute name="text" value="UI_KrustysLastGaspOnline_OutroPanel_2" localise="true" />
					<Attribute name="color" value="000000FF" />
				</AttributeSet>
				<AttributeSet name="panel">
					<Attribute name="image" value="BSE_KrustysLastGaspOnline_MiniEventPanel_Outro" />
				</AttributeSet>
				<AttributeSet name="playButton">
					<Attribute name="color" value="0091F1FF" />
				</AttributeSet>
			</Attribute>
		</Action>
	</ShowEventOutro>
	<ExpiryWarning_72hr>
		<Action type="message" text="UI_KrustysLastGaspOnline_72HourExpiry_text" />
		<Action type="setVariable" boolVariable="KrustysLastGaspOnline_PreExpiryDisplayed72" boolValue="true" />
		<Action type="runScript" script="KrustysLastGaspOnline_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_72hr>
	<ExpiryWarning_24hr>
		<Action type="message" text="{FormattedExpiration24}" formatMe="root">
			<FormattingConfig path="KrustysLastGaspOnline_GameConfig:ExpirationFormattedText" />
		</Action>
		<Action type="setVariable" boolVariable="KrustysLastGaspOnline_PreExpiryDisplayed24" boolValue="true" />
		<Action type="runScript" script="KrustysLastGaspOnline_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</ExpiryWarning_24hr>
	<DisplayExpiry>
		<Action type="message" text="UI_KrustysLastGaspOnline_Expired_text" />
		<Action type="clearRecommendations" category="Event" name="KrustysLastGaspOnline_OnePanel" />
		<Action type="runScript" script="KrustysLastGaspOnline_Scripts:OpenMasterQuest" waitUntilComplete="true" />
	</DisplayExpiry>
	<EventCompleted>
		<Action type="setVariable" boolVariable="KrustysLastGaspOnline_Complete" boolValue="true" />
	</EventCompleted>
	<OpenMasterQuest>
		<Action type="checkRequirements">
			<Requirement type="gamestate" state="GameState_QuestProgress" />
			<True>
				<Action type="changeQuestForProgressState" name="KrustysLastGaspOnline_MasterQuest" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_QuestProgress" forceState="true">
					<Extras>
						<Attribute name="questParentDelegate" value="KrustysLastGaspOnline_MasterQuest" />
					</Extras>
				</Action>
			</False>
		</Action>
	</OpenMasterQuest>
	<ShowJobList ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="if">
					<If>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_1" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_1" />
								</Extras>
							</Action>
						</Then>
					</If>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_2" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_2" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_3" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_3" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_4" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_4" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_5" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_5" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_6" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_6" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
					<ElseIf>
						<Requirement type="active quest" quest="KrustysLastGaspOnline_7" />
						<Then>
							<Action type="gotoState" state="GameState_SpecialEventJobList">
								<Extras>
									<Attribute name="SpecialEvent" value="KrustysLastGaspOnline_Event" />
									<Attribute name="JobList" value="KrustysLastGaspOnline_JL_7" />
								</Extras>
							</Action>
						</Then>
					</ElseIf>
				</Action>
			</True>
			<False>
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</ShowJobList>
	<CheckIfEnd ignoreHUD="true">
		<Action type="checkRequirements">
			<Requirement type="quest" quest="KrustysLastGaspOnline_EventExpired" />
			<True>
				<Action type="setVariable" boolVariable="KrustysLastGaspOnline_Expired" boolValue="true" />
			</True>
		</Action>
	</CheckIfEnd>
	<LastGaspStage_Tapped parallel="true" ignoreHUD="true"></LastGaspStage_Tapped>
	<KrustkateersSet_Tapped parallel="true" ignoreHUD="true"></KrustkateersSet_Tapped>
	<KlubKrustyPool_Tapped parallel="true" ignoreHUD="true"></KlubKrustyPool_Tapped>
	<AutoApply_SideshowLisaSkin_End parallel="true">
		<Action type="scheduleScript" mode="create" script="KrustysLastGaspOnline_Scripts:AutoApply_SideshowLisaSkin_Delayed" relativeTime="2s" localLandOnly="true" shouldSave="true" />
	</AutoApply_SideshowLisaSkin_End>
	<AutoApply_SideshowLisaSkin_Delayed ignoreHUD="true">
		<Action type="consumable" consumable="SideshowLisa" fromInventory="true" />
		<Action type="pantoobject" character="Lisa" />
	</AutoApply_SideshowLisaSkin_Delayed>
	<RecommendStoreCategory ignoreHUD="true">
		<Action type="reparseBuildMenu" />
		<Action type="clearRecommendations" category="Event" name="KrustysLastGaspOnline_OnePanel" />
		<Action type="recommendation" recType="Event" name="KrustysLastGaspOnline_OnePanel" eventName="UI_KrustysLastGaspOnline_Title" limitedTime="true" limitedTimeEndDate="KrustysLastGaspOnline_End" />
		<Action type="focusOnMenuItem" menuCategory="FrontPage" />
		<Action type="reparseBuildMenu" />
	</RecommendStoreCategory>
	<EnterStoreCategory ignoreHUD="true">
		<Action type="focusOnMenuItem" menuCategory="KrustysLastGaspOnline_Category" />
	</EnterStoreCategory>
</ScriptPackage>