	<Selector name="XMAS2017_CraftingCurrencyPerFriendAction" type="table" key="FriendActionsCount">
        0,selector XMAS2017_CraftingCurrency_SocialUpgrade,
        30,0,
    </Selector>
	<Selector name="XMAS2017_CraftingCurrency_SocialUpgrade" type="table" key="XMAS2017_SocialUpgrade_Purchased">
		<TableEntry key="0" value="__XMAS2017_GameConfig:TappableCraftReward:SocialAmount__" />
