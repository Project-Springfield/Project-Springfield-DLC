					</AttributeSet>
					<AttributeSet>
						<Requirement type="reqList" location="XMAS2016_Requirements:ActTwo" />
						<Attribute name="icon" value="ico_cur_xmas2016_chickenfingers_sm" />
						<Attribute name="text" formula="Selector::getNextValue('XMASSixteenGnomeChickenFingersRewards')" />
