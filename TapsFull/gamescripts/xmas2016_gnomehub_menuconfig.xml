				<Attribute name="color" value="0079E0FF" />
			</AttributeSet>
			<AttributeSet name="PlayBtnArrowLeft">
				<Attribute name="color" value="0079E0FF" />
			</AttributeSet>
			<AttributeSet name="CooltimeGaugeBase">
				<Attribute name="color" value="88FFFF44" />
				<Attribute name="color" value="b73affff">
					<Requirement type="formula" formula="System::evaluate('Minigame::SciFighter::Ready')" />
				</Attribute>
			</AttributeSet>
			<AttributeSet name="CooltimeGaugeFill">
				<Attribute name="color" value="00FFFFFF" />
				<Attribute name="color" value="b73affff">
					<Requirement type="formula" formula="System::evaluate('Minigame::SciFighter::Ready')" />
				</Attribute>
