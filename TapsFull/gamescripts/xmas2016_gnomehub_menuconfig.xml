			<AttributeSet name="HasGuide">
				<Attribute name="active" value="true" />
			</AttributeSet>
			<AttributeSet name="CanUpgrade">
				<Attribute name="active" formula="lessThan(GnomeLevel, GnomeLevelCap)" />
			</AttributeSet>
			<AttributeSet name="MaxLevelText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_MaxLevel" localise="true" />
			</AttributeSet>
			<AttributeSet name="TitleText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_TitleText" localise="true" />
				<Attribute name="textReplacements">
					<AttributeSet>
						<Attribute name="index" value="0" />
						<Attribute name="text" formula="GnomeLevel" />
					</AttributeSet>
					<AttributeSet>
						<Attribute name="index" value="1" />
						<Attribute name="text" formula="GnomeLevelCap" />
					</AttributeSet>
				</Attribute>
				<Attribute name="font" value="eFont_Special" />
				<Attribute name="color" value="FFFFFFFF" />
