				<Attribute name="color" value="FFFFFFFF" />
			</AttributeSet>
			<AttributeSet name="GnomeInBoxTitleText">
				<Attribute name="text" value="UI_XMAS2016_GnomeHub_InBox_text" localise="true" />
			</AttributeSet>
			<AttributeSet name="GnomeBoxGraphic">
				<Attribute name="image" value="ico_xmas2016_gnomeinyourhome_gnomebox_close" />
