	<ButtonsConfig>
		<Attribute name="Buttons">
			<AttributeSet name="UpgradeButton">
				<Attribute name="script" value="XMAS2016_GnomeScripts:TryUpgradeGnome" />
			</AttributeSet>
			<AttributeSet name="ExitButton">
				<Attribute name="script" value="MiscScripts:GoToInGameWithSound" />
			</AttributeSet>
			<AttributeSet name="HelpButton">
				<Attribute name="script" value="XMAS2016_Scripts:GuidePanel_GnomeInYourHome_WithClick" />
			</AttributeSet>
			<AttributeSet name="FindGnomeButton">
				<Attribute name="script" value="XMAS2016_GnomeScripts:FindGnome" />
			</AttributeSet>
		</Attribute>
	</ButtonsConfig>
	<ProgressBarConfig>
		<Attribute name="ProgressBars">
			<AttributeSet name="ProgBarFill">
				<Attribute name="timeRange">
					<AttributeSet>
						<Attribute name="startTime" value="GnomeCooldownStart" />
						<Attribute name="endTime" value="GnomeCooldownEnd" />
					</AttributeSet>
				</Attribute>
				<Attribute name="reloadWhenComplete" value="true" />
				<Attribute name="progressBarComponent" value="ProgBarFill" />
				<Attribute name="timeTextComponent" value="ProgressText" />
			</AttributeSet>
		</Attribute>
	</ProgressBarConfig>
