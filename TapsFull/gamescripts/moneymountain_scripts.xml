<ScriptPackage>
	<IncrementNextMoneyMountainLevel parallel="true">
		<Action type="setVariable" intVariable="NextMoneyMountainLevel" intValue="MoneyMountainLevel+1" />
		<Action type="completeJob" name="ThrowPocketfulsOfMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
		<Action type="completeJob" name="ShovelMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
		<Action type="completeJob" name="DumpLoadsOfMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
	</IncrementNextMoneyMountainLevel>
	<MoneyMountainLevelUp>
		<Action type="setVariable" intVariable="MoneyMountainLevel" intValue="MoneyMountainLevel+1" />
		<Action type="sound" sound="MoneyMountain_Upgrade01.caf" />
		<Action type="anim-building-oneshot" postdraw="true" nowait="true" requiresInGame="true">
			<OneShot name="regular" file="MoneyMountainUpGrade" animation="Castle_upGrade_11x11" x="0" y="0" />
		</Action>
		<Action type="delay" time="2" />
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="formula" formula="and(greaterThan(MoneyMountainLevel,0),lessThan(MoneyMountainLevel,11))" />
			</Requirements>
			<True>
				<Action type="runScript" script="MoneyMountainLevelUpPanel" package="MoneyMountain_Scripts" />
			</True>
		</Action>
	</MoneyMountainLevelUp>
	<CompleteMoneyMountainJobs>
		<Action type="completeJob" name="ThrowPocketfulsOfMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
		<Action type="completeJob" name="ShovelMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
		<Action type="completeJob" name="DumpLoadsOfMoney" giveRewardsRefunds="true" forceRefundOrCancel="true" />
	</CompleteMoneyMountainJobs>
	<ShowCurrentMoneyMountainQuest>
		<Action type="gotoState" state="GameState_QuestProgress">
			<Extras>
				<Attribute name="questParentDelegate" value="MoneyMountainMaster" />
			</Extras>
		</Action>
	</ShowCurrentMoneyMountainQuest>
	<MoneyMountainCollectCurrencyGoTo>
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="checkRequirements">
					<Requirements>
						<Requirement type="building" building="MoneyMountain" not="true" />
					</Requirements>
					<True>
						<Action type="focusOnMenuItem" itemName="MoneyMountain" inventory="true" />
					</True>
					<False>
						<Action type="checkRequirements">
							<Requirements>
								<Requirement type="building" building="PowerPlant" not="true" />
							</Requirements>
							<True>
								<Action type="focusOnMenuItem" itemName="PowerPlant" inventory="true" />
							</True>
							<False>
								<Action type="pantoobject" character="Burns" time="0.0" fakeTouch="false" blockTouches="false" />
								<Action type="touchCharacter" character="Burns" />
							</False>
						</Action>
					</False>
				</Action>
			</True>
			<False>
				<Action type="userConfirmation" text="UI_TravelToSpringfieldForQuest" title="BLD_KrustylandShuttleToSpringfield_name" var="confirm_gotoSprinfield" />
				<Action type="checkExpression" expression="confirm_gotoSprinfield">
					<ExpressionCase value="1">
						<Action type="gotoland" land="Springfield" />
					</ExpressionCase>
				</Action>
			</False>
		</Action>
	</MoneyMountainCollectCurrencyGoTo>
