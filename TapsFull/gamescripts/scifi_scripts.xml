	<SciFiLandLoad parallel="true" runQueueState="RunInAnyState">
		<Action type="if">
			<If expression="and(System::evaluate('Game::State::GameState_CustomTouch'), greaterThan(SF_FRAMECNT, 0))">
				<Then>
					<Action type="if">
						<If expression="and(SF_PAUSED==0, SF_INMAIN==1)">
							<Then>
								<Action type="actionSequence" script="SciFi_Scripts_SciFighter:QuitGame" />
							</Then>
						</If>
					</Action>
				</Then>
			</If>
			<Else>
				<Then>
					<Action type="updateVariable" disableEvents="true" variable="SF_STARTED" value="0" />
				</Then>
			</Else>
		</Action>
		<Action type="program">
			<Program><![CDATA[
                    #program;
                    
                    var anomTapped = User::getVariable('AnomaliesTapped');
                    if(anomTapped >=1)
                    {
                        var anomTappedTime = User::getVariable('AnomalyTappedTime');
                        var diff = floor((time() - anomTappedTime) / 7200);
                        User::setVariable('AnomaliesTapped',max(0,anomTapped-diff),false);
                        User::setVariable('AnomalyTappedTime',anomTappedTime+(7200*diff),false);
                    }
                    
                    ]]></Program>
		</Action>
	</SciFiLandLoad>
