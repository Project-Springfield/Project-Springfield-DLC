<ScriptPackage>
	<SpringfieldDam_Tapped parallel="true">
		<Action type="anim-building-oneshot" postdraw="true" onBuilding="SpringfieldDam" nowait="true" panCamera="false">
			<OneShot name="regular" file="springfielddam_overlay01" animation="active_tap" x="0" y="0" />
		</Action>
	</SpringfieldDam_Tapped>
	<GoToCecilOrTownHall>
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="building" building="TownHall" />
			</Requirements>
			<True>
				<Action type="actionSequence" script="MiscScripts:ForceSpringfield">
					<FormatToken key="script" value="AspirationalGrind_Scripts:GoToCecil_Impl" />
				</Action>
			</True>
			<False>
				<Action type="actionSequence" script="MiscScripts:ForceSpringfield">
					<FormatToken key="script" value="AspirationalGrind_Scripts:GoToTownHall" />
				</Action>
			</False>
		</Action>
	</GoToCecilOrTownHall>
	<GoToTownHall>
		<Action type="returnToInGameState" waitForState="true" />
		<Action type="if">
			<If>
				<Requirement type="place building" building="TownHall" />
				<Then>
					<Action type="pantoobject" building="TownHall" />
				</Then>
			</If>
			<ElseIf>
				<Requirement type="building owned" building="TownHall" />
				<Then>
					<Action type="focusOnMenuItem" itemName="TownHall" inventory="true" />
				</Then>
			</ElseIf>
			<Else>
				<Then>
					<Action type="focusOnMenuItem" itemName="TownHall" />
				</Then>
			</Else>
		</Action>
	</GoToTownHall>
	<GoToCecil_Impl>
		<Action type="returnToInGameState" waitForState="true" />
		<Action type="checkRequirements">
			<Requirements>
				<Requirement type="item" item="CecilTerwilliger" checkInventoryOnly="true" />
			</Requirements>
			<True>
				<Action type="focusOnMenuItem" itemName="CecilTerwilliger" inventory="true" />
			</True>
			<False>
				<Action type="pantoobject" character="CecilTerwilliger" fakeTouch="true" fakeTouchAvoidsIndicators="true" time="0" />
			</False>
		</Action>
	</GoToCecil_Impl>
	<OpenQuestTrack>
		<Action type="gotoState" state="GameState_QuestProgress">
			<Extras>
				<Attribute name="questParentDelegate" value="AspirationalGrind_Master" />
			</Extras>
		</Action>
	</OpenQuestTrack>
	<GoToStore_Cecil>
		<Action type="checkRequirements">
			<Requirement type="land" land="Springfield" />
			<True>
				<Action type="focusOnMenuItem" itemName="CecilTerwilliger" menuCategory="Characters" />
			</True>
			<False>
				<Action type="gotoState" state="GameState_InGame" />
				<Action type="pantoobject" building="KrustylandShuttleToSpringfield" fakeTouch="true" />
			</False>
		</Action>
	</GoToStore_Cecil>
	<AspirationalGrind_Teaser_Start>
		<Action type="notification" title="UI_AspirationalGrind_Teaser_name" button="UI_OK" text="UI_AspirationalGrind_Teaser_desc" icon="CubicZirconia_Menu" />
		<Action type="focusOnMenuItem" itemName="CubicZirconia" menuCategory="Decorations" />
